{"ast":null,"code":"import UserService from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      content: null,\n      users: [],\n      selected: null\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getData();\n  },\n  mounted() {\n    UserService.getProfileGuru().then(response => {\n      this.content = response.data;\n    }, error => {\n      this.content = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  },\n  methods: {\n    getData() {\n      UserService.getProfileGuru().then(response => {\n        this.users = response.data.user;\n        console.log(response.data.user);\n      }).catch(e => {\n        console.log(e);\n      });\n      UserService.getProfileGuru().then(response => {\n        this.content = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAgGA,OAAOA,WAAU,MAAO,6BAA6B;AACrD,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,GAAG;IACRb,WAAW,CAACc,cAAc,EAAE,CAACC,IAAI,CAC9BC,QAAQ,IAAK;MACZ,IAAI,CAACd,OAAM,GAAIc,QAAQ,CAACf,IAAI;IAC9B,CAAC,EACAgB,KAAK,IAAK;MACT,IAAI,CAACf,OAAM,GACRe,KAAK,CAACD,QAAO,IACZC,KAAK,CAACD,QAAQ,CAACf,IAAG,IAClBgB,KAAK,CAACD,QAAQ,CAACf,IAAI,CAACiB,OAAO,IAC7BD,KAAK,CAACC,OAAM,IACZD,KAAK,CAACE,QAAQ,EAAE;IACpB,EACD;EACH,CAAC;EACDC,OAAO,EAAE;IACPR,OAAO,GAAG;MACRZ,WAAW,CAACc,cAAc,EAAC,CACxBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACb,KAAI,GAAIa,QAAQ,CAACf,IAAI,CAACS,IAAI;QAC/BW,OAAO,CAACC,GAAG,CAACN,QAAQ,CAACf,IAAI,CAACS,IAAI,CAAC;MACjC,CAAC,EACAa,KAAK,CAAEC,CAAC,IAAK;QACZH,OAAO,CAACC,GAAG,CAACE,CAAC,CAAC;MAChB,CAAC,CAAC;MAEJxB,WAAW,CAACc,cAAc,EAAC,CACxBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACd,OAAM,GAAIc,QAAQ,CAACf,IAAI;MAC9B,CAAC,EACAsB,KAAK,CAAEC,CAAC,IAAK;QACZH,OAAO,CAACC,GAAG,CAACE,CAAC,CAAC;MAChB,CAAC,CAAC;IACN;EACF;AACF,CAAC","names":["UserService","data","content","users","selected","computed","currentUser","$store","state","auth","user","created","getData","mounted","getProfileGuru","then","response","error","message","toString","methods","console","log","catch","e"],"sourceRoot":"","sources":["D:\\Coding\\SISTEM-INFORMASI-SEKOLAH-SMKN-1-JETIS-MASTER\\frontend\\src\\components\\DataNilai\\DataSiswaGuru.vue"],"sourcesContent":["<template>\n  <div>\n    <section class=\"content-header\">\n      <div class=\"container-fluid\">\n        <div class=\"row mb-2\">\n          <div class=\"col-sm-6\">\n            <h1>Data Siswa</h1>\n          </div>\n          <div class=\"col-sm-6\">\n            <ol class=\"breadcrumb float-sm-right\">\n              <li class=\"breadcrumb-item\">\n                <router-link to=\"/dashboard\">Dashboard</router-link>\n              </li>\n              <li class=\"breadcrumb-item active\">Data Siswa</li>\n            </ol>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <section class=\"content\">\n      <div class=\"container-fluid\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h3 class=\"card-title\"></h3>\n              </div>\n              <form>\n                <div class=\"card-body\">\n                  <div class=\"form-group row\">\n                    <label class=\"col-sm-3 col-form-label\" for=\"kelas\"\n                      >Pilih Kelas:</label\n                    >\n                    <div class=\"col-sm-9\">\n                      <select\n                        v-if=\"users.guru && users.guru.kelas\"\n                        class=\"form-control select2\"\n                        v-model=\"selected\"\n                      >\n                        <option\n                          v-for=\"tes in users.guru.kelas\"\n                          :key=\"tes.id\"\n                          :value=\"tes\"\n                        >\n                          {{ tes.name_kelas }}\n                        </option>\n                      </select>\n                    </div>\n                  </div>\n                </div>\n              </form>\n\n              <div v-if=\"selected\">\n                <div class=\"card-body\">\n                  <div class=\"table-responsive\">\n                    <table\n                      id=\"myTable\"\n                      class=\"table table-bordered table-hover\"\n                    >\n                      <thead>\n                        <tr>\n                          <th class=\"align-middle text-center\">No</th>\n                          <th class=\"align-middle text-center\">NIS</th>\n                          <th class=\"align-middle text-center\">Nama</th>\n                        </tr>\n                      </thead>\n                      <tbody>\n                        <tr\n                          v-for=\"(child, index) in selected.siswa\"\n                          :key=\"index.id\"\n                        >\n                          <td class=\"align-middle text-center\">\n                            {{ index + 1 }}\n                          </td>\n                          <td class=\"align-middle text-center\">\n                            {{ child.nis }}\n                          </td>\n                          <td class=\"align-middle text-center\">\n                            {{ child.name }}\n                          </td>\n                        </tr>\n                      </tbody>\n                    </table>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport UserService from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      content: null,\n      users: [],\n      selected: null,\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    },\n  },\n  created() {\n    this.getData();\n  },\n  mounted() {\n    UserService.getProfileGuru().then(\n      (response) => {\n        this.content = response.data;\n      },\n      (error) => {\n        this.content =\n          (error.response &&\n            error.response.data &&\n            error.response.data.message) ||\n          error.message ||\n          error.toString();\n      }\n    );\n  },\n  methods: {\n    getData() {\n      UserService.getProfileGuru()\n        .then((response) => {\n          this.users = response.data.user;\n          console.log(response.data.user);\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n\n      UserService.getProfileGuru()\n        .then((response) => {\n          this.content = response.data;\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}