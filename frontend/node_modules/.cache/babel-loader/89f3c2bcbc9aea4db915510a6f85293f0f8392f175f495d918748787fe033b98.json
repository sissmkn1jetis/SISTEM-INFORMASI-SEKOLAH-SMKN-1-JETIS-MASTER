{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Dashboard\",\n  data() {\n    return {\n      hourRotation: 0,\n      minuteRotation: 0,\n      secondRotation: 0\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  mounted() {\n    if (!this.currentUser) {\n      this.$router.push(\"/login\");\n    }\n    this.startClock();\n  },\n  methods: {\n    startClock() {\n      this.updateClock();\n      setInterval(this.updateClock, 1000);\n    },\n    updateClock() {\n      const now = new Date();\n      const hours = now.getHours() % 12;\n      const minutes = now.getMinutes();\n      const seconds = now.getSeconds();\n      this.hourRotation = hours * 360 / 12 + minutes * 360 / 60 / 12;\n      this.minuteRotation = minutes * 360 / 60 + seconds * 360 / 60 / 60;\n      this.secondRotation = seconds * 360 / 60;\n    }\n  }\n};","map":{"version":3,"mappings":";AA4HA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;MACfC,cAAc,EAAE,CAAC;MACjBC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAAC,IAAI,CAACL,WAAW,EAAE;MACrB,IAAI,CAACM,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B;IACA,IAAI,CAACC,UAAU,EAAE;EACnB,CAAC;EACDC,OAAO,EAAE;IACPD,UAAU,GAAG;MACX,IAAI,CAACE,WAAW,EAAE;MAClBC,WAAW,CAAC,IAAI,CAACD,WAAW,EAAE,IAAI,CAAC;IACrC,CAAC;IACDA,WAAW,GAAG;MACZ,MAAME,GAAE,GAAI,IAAIC,IAAI,EAAE;MACtB,MAAMC,KAAI,GAAIF,GAAG,CAACG,QAAQ,EAAC,GAAI,EAAE;MACjC,MAAMC,OAAM,GAAIJ,GAAG,CAACK,UAAU,EAAE;MAChC,MAAMC,OAAM,GAAIN,GAAG,CAACO,UAAU,EAAE;MAChC,IAAI,CAACvB,YAAW,GAAKkB,KAAI,GAAI,GAAG,GAAI,EAAC,GAAKE,OAAM,GAAI,GAAG,GAAI,EAAC,GAAI,EAAE;MAClE,IAAI,CAACnB,cAAa,GAAKmB,OAAM,GAAI,GAAG,GAAI,EAAC,GAAKE,OAAM,GAAI,GAAG,GAAI,EAAC,GAAI,EAAE;MACtE,IAAI,CAACpB,cAAa,GAAKoB,OAAM,GAAI,GAAG,GAAI,EAAE;IAC5C;EACF;AACF,CAAC","names":["name","data","hourRotation","minuteRotation","secondRotation","computed","currentUser","$store","state","auth","user","mounted","$router","push","startClock","methods","updateClock","setInterval","now","Date","hours","getHours","minutes","getMinutes","seconds","getSeconds"],"sourceRoot":"","sources":["D:\\filee\\SISTEM-INFORMASI-SEKOLAH-SMKN-1-JETIS-MASTER\\frontend\\src\\components\\Dashboard.vue"],"sourcesContent":["<template>\r\n  <!-- <div class=\"container\">\r\n    <header class=\"jumbotron\">\r\n      <h3>\r\n        <strong>{{ currentUser.username }}</strong> Profile\r\n      </h3>\r\n    </header>\r\n    <p>\r\n      <strong>Token:</strong>\r\n      {{ currentUser.accessToken.substring(0, 20) }} ...\r\n      {{ currentUser.accessToken.substr(currentUser.accessToken.length - 20) }}\r\n    </p>\r\n    <p>\r\n      <strong>Id:</strong>\r\n      {{ currentUser.id }}\r\n    </p>\r\n    <p>\r\n      <strong>Email:</strong>\r\n      {{ currentUser.email }}\r\n    </p>\r\n    <strong>Authorities:</strong>\r\n    <ul>\r\n      <li v-for=\"role in currentUser.roles\" :key=\"role\">{{ role }}</li>\r\n    </ul>\r\n  </div> -->\r\n  <div>\r\n    <!-- Content Header (Page header) -->\r\n    <section class=\"content-header\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row mb-2\">\r\n          <div class=\"col-sm-6\">\r\n            <h1>Dashboard</h1>\r\n          </div>\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Dashboard</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Main content -->\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-8\">\r\n            <div v-if=\"currentUser.role === 'admin'\">\r\n              <!-- Konten untuk Admin -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Admin</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  <p class=\"welcome-msg\">\r\n                    Selamat Datang <strong>{{ currentUser.username }}</strong\r\n                    >, Anda Login Sebagai\r\n                    <strong>{{ currentUser.role }}</strong>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else-if=\"currentUser.role === 'guru'\">\r\n              <!-- Konten untuk Guru -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Guru</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  Selamat Datang\r\n                  <strong>{{ currentUser.username }}</strong> Anda Login Sebagai\r\n                  <strong>{{ currentUser.role }}</strong>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else-if=\"currentUser.role === 'walas'\">\r\n              <!-- Konten untuk Wali Kelas -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Wali Kelas</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  Selamat Datang\r\n                  <strong>{{ currentUser.username }}</strong> Anda Login Sebagai\r\n                  <strong>{{ currentUser.role }}</strong>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else>\r\n              <p>Anda tidak memiliki akses ke dashboard ini.</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-md-4\">\r\n            <!-- Tampilan jam dan tanggal di samping card utama -->\r\n            <div class=\"card\">\r\n              <div class=\"card-body text-center\">\r\n                <div class=\"analog-clock\">\r\n                  <div\r\n                    class=\"hour-hand\"\r\n                    :style=\"{ transform: `rotate(${hourRotation}deg)` }\"\r\n                  ></div>\r\n                  <div\r\n                    class=\"minute-hand\"\r\n                    :style=\"{ transform: `rotate(${minuteRotation}deg)` }\"\r\n                  ></div>\r\n                  <div\r\n                    class=\"second-hand\"\r\n                    :style=\"{ transform: `rotate(${secondRotation}deg)` }\"\r\n                  ></div>\r\n                  <div class=\"clock-face\"></div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Dashboard\",\r\n  data() {\r\n    return {\r\n      hourRotation: 0,\r\n      minuteRotation: 0,\r\n      secondRotation: 0,\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  mounted() {\r\n    if (!this.currentUser) {\r\n      this.$router.push(\"/login\");\r\n    }\r\n    this.startClock();\r\n  },\r\n  methods: {\r\n    startClock() {\r\n      this.updateClock();\r\n      setInterval(this.updateClock, 1000);\r\n    },\r\n    updateClock() {\r\n      const now = new Date();\r\n      const hours = now.getHours() % 12;\r\n      const minutes = now.getMinutes();\r\n      const seconds = now.getSeconds();\r\n      this.hourRotation = (hours * 360) / 12 + (minutes * 360) / 60 / 12;\r\n      this.minuteRotation = (minutes * 360) / 60 + (seconds * 360) / 60 / 60;\r\n      this.secondRotation = (seconds * 360) / 60;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Gaya tambahan untuk membuat tampilan jam analog yang lebih baik */\r\n.analog-clock {\r\n  position: relative;\r\n  width: 200px;\r\n  height: 200px;\r\n  border: 10px solid #333;\r\n  border-radius: 50%;\r\n}\r\n\r\n.hour-hand,\r\n.minute-hand,\r\n.second-hand {\r\n  position: absolute;\r\n  background: #333;\r\n}\r\n\r\n.hour-hand {\r\n  width: 4px;\r\n  height: 60px;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform-origin: bottom center;\r\n  transform: translateX(-50%) rotate(0deg);\r\n  transition: transform 0.5s cubic-bezier(0.4, 2.1, 0.1, 0.97);\r\n}\r\n\r\n.minute-hand {\r\n  width: 2px;\r\n  height: 80px;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform-origin: bottom center;\r\n  transform: translateX(-50%) rotate(0deg);\r\n  transition: transform 0.5s cubic-bezier(0.4, 2.1, 0.1, 0.97);\r\n}\r\n\r\n.second-hand {\r\n  width: 1px;\r\n  height: 90px;\r\n  background: red;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform-origin: bottom center;\r\n  transform: translateX(-50%) rotate(0deg);\r\n  transition: transform 0.1s linear;\r\n}\r\n\r\n.clock-face {\r\n  position: absolute;\r\n  width: 12px;\r\n  height: 12px;\r\n  background: #333;\r\n  border-radius: 50%;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n\r\n@keyframes fadeIn {\r\n  0% {\r\n    opacity: 0;\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}