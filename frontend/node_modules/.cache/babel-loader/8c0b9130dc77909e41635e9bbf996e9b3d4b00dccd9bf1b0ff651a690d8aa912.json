        </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"nama\"\r\n                      >Nama:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users.wala\">{{ users.wala.guru.nama }}</p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"kelas\"\r\n                      >Kelas:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users.wala\">{{ users.wala.kelas.name_kelas }}</p>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n              <div v-if=\"users.wala && users.wala.kelas.siswa\">\r\n                <div class=\"card-body\">\r\n                  <div class=\"table-responsive\">\r\n                    <table\r\n                      id=\"myTable\"\r\n                      class=\"table table-bordered table-hover\"\r\n                    >\r\n                      <thead>\r\n                        <tr>\r\n                          <th class=\"align-middle text-center\">No</th>\r\n                          <th class=\"align-middle text-center\">NIS</th>\r\n                          <th class=\"align-middle text-center\">Nama</th>\r\n                        </tr>\r\n                      </thead>\r\n                      <tbody>\r\n                        <tr\r\n                          v-for=\"(child, index) in users.wala.kelas.siswa\"\r\n                          :key=\"index.id\"\r\n                        >\r\n                          <td class=\"align-middle text-center\">\r\n                            {{ index + 1 }}\r\n                          </td>\r\n                          <td class=\"align-middle text-center\">\r\n                            {{ child.nis }}\r\n                          </td>\r\n                          <td class=\"align-middle text-center\">\r\n                            {{ child.name }}\r\n                          </td>\r\n                        </tr>\r\n                      </tbody>\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from \"../../services/user.service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      content: null,\r\n      users: [],\r\n      selected: null,\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n  mounted() {\r\n    UserService.getProfileWalas().then(\r\n      (response) => {\r\n        this.content = response.data;\r\n      },\r\n      (error) => {\r\n        this.content =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n  methods: {\r\n    getData() {\r\n      UserService.getProfileWalas()\r\n        .then((response) => {\r\n          this.users = response.data.profiledata;\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n\r\n      UserService.getProfileWalas()\r\n        .then((response) => {\r\n          this.content = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}