ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <div class=\"card\">\r\n              <div class=\"card-header\">\r\n                <h3 class=\"card-title\"></h3>\r\n              </div>\r\n              <form>\r\n                <div class=\"card-body\">\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"nip\"\r\n                      >NIP:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users.user\">\r\n                        {{ users.user.wala.guru.nip }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"nama\"\r\n                      >Nama:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users.user\">\r\n                        {{ users.user.wala.guru.nama }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"kelas\"\r\n                      >Kelas:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users.user\">\r\n                        {{ users.user.wala.kelas.name_kelas }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n              <select v-model=\"selectedMapel\">\r\n                <option value=\"\">Pilih Mata Pelajaran</option>\r\n                <option\r\n                  v-for=\"guru in users.filterdataguru\"\r\n                  :key=\"guru.id\"\r\n                  :value=\"guru.id\"\r\n                >\r\n                  {{ guru.mapel.name_mapel }} - {{ guru.nama }}\r\n                </option>\r\n              </select>\r\n\r\n              <div v-if=\"selectedMapel\">\r\n                <h2>Data Siswa dan Nilai</h2>\r\n                <ul>\r\n                  <li v-for=\"siswa in siswaKelas\" :key=\"siswa.id\">\r\n                    {{ siswa.name }} ({{ siswa.nis }}) - Nilai:\r\n                    {{ getNilaiForSiswa(siswa.id) }}\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from \"../../services/user.service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      content: null,\r\n      users: [],\r\n      selected: null,\r\n      selectedMapel: \"\",\r\n      siswaKelas: [],\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n  mounted() {\r\n    UserService.getProfileWalas().then(\r\n      (response) => {\r\n        this.content = response.data;\r\n      },\r\n      (error) => {\r\n        this.content =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n  methods: {\r\n    getData() {\r\n      UserService.getProfileWalas()\r\n        .then((response) => {\r\n          this.users = response.data;\r\n          this.siswaKelas = response.data.user.wala.kelas.siswa;\r\n          this.nkData = response.data.nk;\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n\r\n      UserService.getProfileWalas()\r\n        .then((response) => {\r\n          this.content = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    getNilaiForSiswa(siswaId) {\r\n      const nkItem = this.nkData.find((item) => item.siswaId === siswaId);\r\n      return nkItem ? nkItem.totalNilai : \"-\";\r\n    },\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}