{"ast":null,"code":"import UserService from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      content: null,\n      users: [],\n      selected: null,\n      selectedMapel: \"\",\n      siswaKelas: []\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getData();\n  },\n  mounted() {\n    UserService.getProfileWalas().then(response => {\n      this.content = response.data;\n    }, error => {\n      this.content = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  },\n  methods: {\n    getData() {\n      UserService.getProfileWalas().then(response => {\n        this.users = response.data;\n        this.siswaKelas = response.data.user.wala.kelas.siswa;\n        this.nkData = response.data.nk;\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n      UserService.getProfileWalas().then(response => {\n        this.content = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getNilaiForSiswa(siswaId) {\n      const nkItem = this.nkData.find(item => item.siswaId === siswaId);\n      return nkItem ? nkItem.totalNilai : \"-\";\n    }\n  }\n};","map":{"version":3,"mappings":"AA2FA,OAAOA,WAAU,MAAO,6BAA6B;AACrD,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,IAAI;MACdC,aAAa,EAAE,EAAE;MACjBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,GAAG;IACRf,WAAW,CAACgB,eAAe,EAAE,CAACC,IAAI,CAC/BC,QAAQ,IAAK;MACZ,IAAI,CAAChB,OAAM,GAAIgB,QAAQ,CAACjB,IAAI;IAC9B,CAAC,EACAkB,KAAK,IAAK;MACT,IAAI,CAACjB,OAAM,GACRiB,KAAK,CAACD,QAAO,IACZC,KAAK,CAACD,QAAQ,CAACjB,IAAG,IAClBkB,KAAK,CAACD,QAAQ,CAACjB,IAAI,CAACmB,OAAO,IAC7BD,KAAK,CAACC,OAAM,IACZD,KAAK,CAA