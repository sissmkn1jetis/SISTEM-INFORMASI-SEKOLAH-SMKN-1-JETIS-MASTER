{"ast":null,"code":"import UserService from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      content: null,\n      users: [],\n      selectedMapel: null,\n      filterguru: [],\n      nkData: []\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getData();\n  },\n  mounted() {\n    UserService.getProfileWalas().then(response => {\n      this.content = response.data;\n    }, error => {\n      this.content = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  },\n  methods: {\n    getData() {\n      UserService.getProfileWalas().then(response => {\n        this.users = response.data.user;\n        this.filterguru = response.data.filterdataguru;\n        this.nkData = response.data.nk;\n\n        // const userKelasId = this.users.wala.kelas.id;\n        // this.filterguru = response.data.filterdataguru.filter((guru) => {\n        //   return guru.kelas.id === userKelasId;\n        // });\n        console.log(response.data.filterdataguru.kelas);\n      }).catch(e => {\n        console.log(e);\n      });\n      UserService.getProfileWalas().then(response => {\n        this.content = response.data.user;\n        // const userKelasId = this.content.wala.kelas.id;\n        // this.filterguru = response.data.filterdataguru.filter((guru) => {\n        //   return guru.kelas.id === userKelasId;\n        // });\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    onMapelSelect() {\n      // Lakukan pemfilteran data siswa dan nilai berdasarkan mata pelajaran yang dipilih\n      // Dapatkan id kelas dari user\n      const userKelasId = this.users.wala.kelas.id;\n\n      // Lakukan pemfilteran data guru berdasarkan id kelas\n      this.filterguru = this.users.filterdataguru.filter(guru => {\n        return guru.kelas.id === userKelasId;\n      });\n\n      // Dapatkan data siswa dan nilai sesuai mata pelajaran yang dipilih\n      this.nkData = this.content.nk.filter(nilai => {\n        return nilai.guruId === this.selectedMapel.guru.id;\n      });\n    },\n    getNilaiNK(siswaId, jenis, i) {\n      const nilai = this.nkData.find(item => item.siswaId === siswaId);\n      if (nilai) {\n        return nilai[jenis + i] || \"-\";\n      }\n      return \"-\";\n    }\n  }\n};","map":{"version":3,"mappings":"AA0LA,OAAOA,WAAU,MAAO,6BAA6B;AACrD,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE,IAAI;MACnBC,UAAU,EAAE,EAAE;MACdC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,GAAG;IACRf,WAAW,CAACgB,eAAe,EAAE,CAACC,IAAI,CAC/BC,QAAQ,IAAK;MACZ,IAAI,CAAChB,OAAM,GAAIgB,QAAQ,CAACjB,IAAI;IAC9B,CAAC,EACAkB,KAAK,IAAK;MACT,IAAI,CAACjB,OAAM,GACRiB,KAAK,CAACD,QAAO,IACZC,KAAK,CAACD,QAAQ,CAACjB,IAAG,IAClBkB,KAAK,CAACD,QAAQ,CAACjB,IAAI,CAACmB,OAAO,IAC7BD,KAAK,CAACC,OAAM,IACZD,KAAK,CAACE,QAAQ,EAAE;IACpB,EACD;EACH,CAAC;EACDC,OAAO,EAAE;IACPR,OAAO,GAAG;MACRd,WAAW,CAACgB,eAAe,EAAC,CACzBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACf,KAAI,GAAIe,QAAQ,CAACjB,IAAI,CAACW,IAAI;QAC/B,IAAI,CAACP,UAAS,GAAIa,QAAQ,CAACjB,IAAI,CAACsB,cAAc;QAC9C,IAAI,CAACjB,MAAK,GAAIY,QAAQ,CAACjB,IAAI,CAACuB,EAAE;;QAE9B;QACA;QACA;QACA;QACAC,OAAO,CAACC,GAAG,CAACR,QAAQ,CAACjB,IAAI,CAACsB,cAAc,CAACI,KAAK,CAAC;MACjD,CAAC,EACAC,KAAK,CAAEC,CAAC,IAAK;QACZJ,OAAO,CAACC,GAAG,CAACG,CAAC,CAAC;MAChB,CAAC,CAAC;MAEJ7B,WAAW,CAACgB,eAAe,EAAC,CACzBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAAChB,OAAM,GAAIgB,QAAQ,CAACjB,IAAI,CAACW,IAAI;QACjC;QACA;QACA;QACA;MACF,CAAC,EACAgB,KAAK,CAAEC,CAAC,IAAK;QACZJ,OAAO,CAACC,GAAG,CAACG,CAAC,CAAC;MAChB,CAAC,CAAC;IACN,CAAC;IAEDC,aAAa,GAAG;MACd;MACA;MACA,MAAMC,WAAU,GAAI,IAAI,CAAC5B,KAAK,CAAC6B,IAAI,CAACL,KAAK,CAACM,EAAE;;MAE5C;MACA,IAAI,CAAC5B,UAAS,GAAI,IAAI,CAACF,KAAK,CAACoB,cAAc,CAACW,MAAM,CAAEC,IAAI,IAAK;QAC3D,OAAOA