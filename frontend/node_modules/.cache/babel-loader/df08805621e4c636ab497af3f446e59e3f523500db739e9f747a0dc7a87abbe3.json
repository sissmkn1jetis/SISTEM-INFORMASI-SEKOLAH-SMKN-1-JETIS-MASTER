              <h3 class=\"card-title\"></h3>\r\n              </div>\r\n              <form>\r\n                <div class=\"card-body\">\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"nip\"\r\n                      >NIP:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users && users.wala\">\r\n                        {{ users.wala.guru.nip }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"nama\"\r\n                      >Nama:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users && users.wala\">\r\n                        {{ users.wala.guru.nama }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"kelas\"\r\n                      >Kelas:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users && users.wala\">\r\n                        {{ users.wala.kelas.name_kelas }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"mapel\"\r\n                      >Pilih Mata Pelajaran:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <select\r\n                        class=\"form-control select2\"\r\n                        v-if=\"filterguru\"\r\n                        v-model=\"selectedMapel\"\r\n                        @change=\"onMapelSelect\"\r\n                      >\r\n                        <option\r\n                          v-for=\"tes in filterguru\"\r\n                          :key=\"tes.id\"\r\n                          :value=\"tes\"\r\n                        >\r\n                          {{ tes.mapel.name_mapel }} - {{ tes.nama }}\r\n                        </option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n\r\n              <div v-if=\"selectedMapel\">\r\n                <div class=\"card-body\">\r\n                  <div class=\"table-responsive\">\r\n                    <table\r\n                      id=\"myTable\"\r\n                      class=\"table table-bordered table-hover\"\r\n                    >\r\n                      <thead>\r\n                        <tr>\r\n                          <th colSpan=\"32\" class=\"text-center\">\r\n                            Nilai Ketrampilan\r\n                          </th>\r\n                        </tr>\r\n                        <tr>\r\n                          <th rowSpan=\"2\" class=\"align-middle\">No</th>\r\n                          <th rowSpan=\"2\" class=\"align-middle text-center\">\r\n                            NIS\r\n                          </th>\r\n                          <th rowSpan=\"2\" class=\"align-middle text-center\">\r\n                            Siswa\r\n                          </th>\r\n                          <th colSpan=\"8\" class=\"text-center\">Proses</th>\r\n                          <th colSpan=\"8\" class=\"text-center\">Proyek</th>\r\n                          <th colSpan=\"8\" class=\"text-center\">Produk</th>\r\n                        </tr>\r\n                        <tr>\r\n                          <th class=\"text-center\">1</th>\r\n                          <th class=\"text-center\">2</th>\r\n                          <th class=\"text-center\">3</th>\r\n                          <th class=\"text-center\">4</th>\r\n                          <th class=\"text-center\">5</th>\r\n                          <th class=\"text-center\">6</th>\r\n                          <th class=\"text-center\">7</th>\r\n                          <th class=\"text-center\">8</th>\r\n                          <th class=\"text-center\">1</th>\r\n                          <th class=\"text-center\">2</th>\r\n                          <th class=\"text-center\">3</th>\r\n                          <th class=\"text-center\">4</th>\r\n                          <th class=\"text-center\">5</th>\r\n                          <th class=\"text-center\">6</th>\r\n                          <th class=\"text-center\">7</th>\r\n                          <th class=\"text-center\">8</th>\r\n                          <th class=\"text-center\">1</th>\r\n                          <th class=\"text-center\">2</th>\r\n                          <th class=\"text-center\">3</th>\r\n                          <th class=\"text-center\">4</th>\r\n                          <th class=\"text-center\">5</th>\r\n                          <th class=\"text-center\">6</th>\r\n                          <th class=\"text-center\">7</th>\r\n                          <th class=\"text-center\">8</th>\r\n                        </tr>\r\n                      </thead>\r\n                      <tbody>\r\n                        <tr\r\n                          v-for=\"(child, index) in selectedMapel.kelas.siswa\"\r\n                          :key=\"child.id\"\r\n                        >\r\n                          <td class=\"align-middle text-center\">\r\n                            {{ index + 1 }}\r\n                          </td>\r\n                          <td class=\"align-middle text-center\">\r\n                            {{ child.nis }}\r\n                          </td>\r\n                          <td class=\"align-middle text-center\">\r\n                            {{ child.name }}\r\n                          </td>\r\n                          <td\r\n                            v-for=\"i in 8\"\r\n                            :key=\"i\"\r\n                            class=\"align-middle text-center\"\r\n                          >\r\n                            {{ getNilaiNK(child.id, \"proses\", i) }}\r\n                          </td>\r\n                          <td\r\n                            v-for=\"i in 8\"\r\n                            :key=\"i\"\r\n                            class=\"align-middle text-center\"\r\n                          >\r\n                            {{ getNilaiNK(child.id, \"proyek\", i) }}\r\n                          </td>\r\n                          <td\r\n                            v-for=\"i in 8\"\r\n                            :key=\"i\"\r\n                            class=\"align-middle text-center\"\r\n                          >\r\n                            {{ getNilaiNK(child.id, \"produk\", i) }}\r\n                          </td>\r\n                        </tr>\r\n                      </tbody>\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from \"../../services/user.service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      content: null,\r\n      users: [],\r\n      selectedMapel: null,\r\n      filterguru: [],\r\n      nkData: [],\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n  mounted() {\r\n    UserService.getProfileWalas().then(\r\n      (response) => {\r\n        this.content = response.data;\r\n      },\r\n      (error) => {\r\n        this.content =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n  methods: {\r\n    getData() {\r\n      UserService.getProfileWalas()\r\n        .then((response) => {\r\n          this.users = response.data.user;\r\n          this.filterguru = response.data.filterdataguru;\r\n          this.nkData = response.data.nk;\r\n\r\n          // const userKelasId = this.users.wala.kelas.id;\r\n          // this.filterguru = response.data.filterdataguru.filter((guru) => {\r\n          //   return guru.kelas.id === userKelasId;\r\n          // });\r\n          console.log(response.data.filterdataguru.kelas);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n\r\n      UserService.getProfileWalas()\r\n        .then((response) => {\r\n          this.content = response.data.user;\r\n          // const userKelasId = this.content.wala.kelas.id;\r\n          // this.filterguru = response.data.filterdataguru.filter((guru) => {\r\n          //   return guru.kelas.id === userKelasId;\r\n          // });\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    onMapelSelect() {\r\n      // Lakukan pemfilteran data siswa dan nilai berdasarkan mata pelajaran yang dipilih\r\n      // Dapatkan id kelas dari user\r\n      const userKelasId = this.users.wala.kelas.id;\r\n\r\n      // Lakukan pemfilteran data guru berdasarkan id kelas\r\n      this.filterguru = this.users.filterdataguru.filter((guru) => {\r\n        return guru.kelas.id === userKelasId;\r\n      });\r\n\r\n      // Dapatkan data siswa dan nilai sesuai mata pelajaran yang dipilih\r\n      this.nkData = this.content.nk.filter((nilai) => {\r\n        return nilai.guruId === this.selectedMapel.guru.id;\r\n      });\r\n    },\r\n    getNilaiNK(siswaId, jenis, i) {\r\n      const nilai = this.nkData.find((item) => item.siswaId === siswaId);\r\n      if (nilai) {\r\n        return nilai[jenis + i] || \"-\";\r\n      }\r\n      return \"-\";\r\n    },\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}