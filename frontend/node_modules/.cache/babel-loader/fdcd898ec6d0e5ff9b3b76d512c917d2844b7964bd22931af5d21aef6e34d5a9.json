                <select\r\n                      class=\"form-control select2\"\r\n                      v-model=\"form.kelasId\"\r\n                      required\r\n                    >\r\n                      <option disabled value=\"\">-- Pilih --</option>\r\n                      <option\r\n                        v-for=\"kelass in kelas\"\r\n                        :key=\"kelass.id\"\r\n                        :value=\"kelass.id\"\r\n                      >\r\n                        {{ kelass.name_kelas }}\r\n                      </option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <label>Siswa</label>\r\n                    <select\r\n                      class=\"form-control select2\"\r\n                      v-model=\"form.siswaId\"\r\n                      required\r\n                    >\r\n                      <option disabled value=\"\">-- Pilih --</option>\r\n                      <option\r\n                        v-for=\"siswa in filterDataSiswa\"\r\n                        :key=\"siswa.id\"\r\n                        :value=\"siswa.id\"\r\n                      >\r\n                        {{ siswa.name }}\r\n                      </option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <label>Semester</label>\r\n                    <select\r\n                      class=\"form-control select2\"\r\n                      v-model=\"form.thnAjarId\"\r\n                      required\r\n                    >\r\n                      <option disabled value=\"\">-- Pilih --</option>\r\n                      <option\r\n                        v-for=\"smt in thn_ajars\"\r\n                        :key=\"smt.id\"\r\n                        :value=\"smt.id\"\r\n                      >\r\n                        {{ smt.semester }}\r\n                      </option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <label>Mata Pelajaran</label>\r\n                    <select\r\n                      class=\"form-control select2\"\r\n                      v-model=\"form.mapelId\"\r\n                      required\r\n                    >\r\n                      <option disabled value=\"\">-- Pilih --</option>\r\n                      <option\r\n                        v-for=\"mapel in mapels\"\r\n                        :key=\"mapel.id\"\r\n                        :value=\"mapel.id\"\r\n                      >\r\n                        {{ mapel.name_mapel }}\r\n                      </option>\r\n                    </select>\r\n                  </div>\r\n                </div>\r\n                <!-- /.card-body -->\r\n\r\n                <div class=\"card-footer\">\r\n                  <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\r\n                </div>\r\n              </form>\r\n            </div>\r\n            <!-- /.card -->\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from \"../services/user.service\";\r\nexport default {\r\n  name: \"Guru\",\r\n  data() {\r\n    return {\r\n      content: null,\r\n      users: null,\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n  mounted() {\r\n    UserService.getProfile().then(\r\n      (response) => {\r\n        this.content = response.data;\r\n      },\r\n      (error) => {\r\n        this.content =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n  methods: {\r\n    getData() {\r\n      UserService.getProfile()\r\n        .then((response) => {\r\n          this.users = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}