{"ast":null,"code":"import UserService from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      content: null,\n      users: {},\n      selectedMapel: null,\n      selectedKelasId: null,\n      selectedSiswa: [],\n      filterguru: [],\n      nkData: []\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getData();\n  },\n  mounted() {\n    UserService.getProfileWalas().then(response => {\n      this.content = response.data;\n    }, error => {\n      this.content = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  },\n  methods: {\n    getData() {\n      UserService.getProfileWalas().then(response => {\n        this.users = response.data.user;\n        this.nkData = response.data.nk;\n        this.filterguru = response.data.filterdataguru;\n\n        // if (this.users && this.users.wala && this.users.wala.kelas) {\n        //   const userKelasId = this.users.wala.kelas.id;\n        //   this.filterguru = response.data.filterdataguru.filter((guru) => {\n        //     return guru.kelas.id === userKelasId;\n        //   });\n        // }\n        // const userKelasId = this.users.wala.kelas.id;\n        // this.filterguru = response.data.filterdataguru.filter((guru) => {\n        //   return guru.kelas.id === userKelasId;\n        // });\n        console.log(\"Users Data:\", this.users); // Tambahkan ini\n        console.log(\"Filter Guru Data:\", this.filterguru); // Tambahkan ini\n        console.log(\"NK Data:\", this.nkData); // Tambahkan ini\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n\n      // UserService.getProfileWalas()\n      //   .then((response) => {\n      //     this.filterguru = response.data.filterdataguru;\n      //     console.log(response.data.filterdataguru);\n      //   })\n      //   .catch((e) => {\n      //     console.log(e);\n      //   });\n    },\n\n    onMapelSelect() {\n      console.log(\"Selected Mapel:\", this.selectedMapel);\n      if (this.selectedMapel && this.selectedMapel.kelas) {\n        console.log(\"Kelas Data:\", this.selectedMapel.kelas);\n        if (this.selectedMapel.kelas.siswa) {\n          console.log(\"Siswa Data:\", this.selectedMapel.kelas.siswa);\n        } else {\n          console.log(\"No Siswa Data Found\");\n        }\n      }\n    },\n    getNilaiNK(siswaId, jenis, i) {\n      const nilai = this.nkData.find(item => item.siswaId === siswaId);\n      if (nilai) {\n        return nilai[jenis + i] || \"-\";\n      }\n      return \"-\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAwLA,OAAOA,WAAU,MAAO,6BAA6B;AACrD,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,CAAC,CAAC;MACTC,aAAa,EAAE,IAAI;MACnBC,eAAe,EAAE,IAAI;MACrBC,aAAa,EAAE,EAAE;MACjBC,UAAU,EAAE,EAAE;MACdC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,GAAG;IACRjB,WAAW,CAACkB,eAAe,EAAE,CAACC,IAAI,CAC/BC,QAAQ,IAAK;MACZ,IAAI,CAAClB,OAAM,GAAIkB,QAAQ,CAACnB,IAAI;IAC9B,CAAC,EACAoB,KAAK,IAAK;MACT,IAAI,CAACnB,OAAM,GACRmB,KAAK,CAACD,QAAO,IACZC,KAAK,CAACD,QAAQ,CAACnB,IAAG,IAClBoB,KAAK,CAACD,QAAQ,CAACnB,IAAI,CAACqB,OAAO,IAC7BD,KAAK,CAACC,OAAM,IACZD,KAAK,CAACE,QAAQ,EAAE;IACpB,EACD;EACH,CAAC;EACDC,OAAO,EAAE;IACPR,OAAO,GAAG;MACRhB,WAAW,CAACkB,eAAe,EAAC,CACzBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACjB,KAAI,GAAIiB,QAAQ,CAACnB,IAAI,CAACa,IAAI;QAC/B,IAAI,CAACN,MAAK,GAAIY,QAAQ,CAACnB,IAAI,CAACwB,EAAE;QAC9B,IAAI,CAAClB,UAAS,GAAIa,QAAQ,CAACnB,IAAI,CAACyB,cAAc;;QAE9C;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACAC,OAAO,CAACC,GAAG,CAAC,aAAa,EAAE,IAAI,CAACzB,KAAK,CAAC,EAAE;QACxCwB,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAE,IAAI,CAACrB,UAAU,CAAC,EAAE;QACnDoB,OAAO,CAACC,GAAG,CAAC,UAAU,EAAE,IAAI,CAACpB,MAAM,CAAC,EAAE;QACtCmB,OAAO,CAACC,GAAG,CAACR,QA