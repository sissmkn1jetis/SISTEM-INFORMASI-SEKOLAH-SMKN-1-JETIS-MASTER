     <div class=\"card-tools\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-success mb-2\"\r\n                    @click=\"showModal\"\r\n                  >\r\n                    Tambah Mata Pelajaran\r\n                  </button>\r\n                </div>\r\n              </div>\r\n              <div class=\"card-body\">\r\n                <table id=\"myTable\" class=\"table table-bordered table-hover\">\r\n                  <thead>\r\n                    <tr>\r\n                      <th class=\"align-middle text-center\">No</th>\r\n                      <th class=\"align-middle text-center\">Mata Pelajaran</th>\r\n                      <th class=\"align-middle text-center\">Action</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"(mapel, index) in mapels\" :key=\"index.id\">\r\n                      <td class=\"align-middle text-center\">\r\n                        {{ index + 1 }}\r\n                      </td>\r\n                      <td class=\"align-middle text-center\">\r\n                        {{ mapel.name_mapel }}\r\n                      </td>\r\n                      <td class=\"align-middle text-center\">\r\n                        <a\r\n                          href=\"#\"\r\n                          class=\"badge bg-info mr-2\"\r\n                          @click=\"showModalEdit(mapel.id)\"\r\n                          id=\"edit\"\r\n                          ><i class=\"fas fa-edit\"></i\r\n                        ></a>\r\n                        <a\r\n                          href=\"#\"\r\n                          class=\"badge bg-danger\"\r\n                          @click=\"deleteData(mapel.id)\"\r\n                          ><i class=\"fas fa-trash-alt\"></i\r\n                        ></a>\r\n                      </td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"modalmuncul\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"modalmuncul1\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-lg\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"statusmodal\"\r\n              >\r\n                Tambah Mata Pelajaran\r\n              </h5>\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"statusmodal\"\r\n              >\r\n                Ubah Mata Pelajaran\r\n              </h5>\r\n              <button\r\n                type=\"button\"\r\n                class=\"close\"\r\n                data-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <span aria-hidden=\"true\" @click=\"resetForm()\">&times;</span>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"card card-default\">\r\n                <!-- /.card-header -->\r\n                <!-- form start -->\r\n                <form\r\n                  @submit.prevent=\"statusmodal ? updateData() : simpanData()\"\r\n                >\r\n                  <div class=\"card-body\">\r\n                    <div class=\"form-group row\">\r\n                      <label class=\"col-sm-3 col-form-label\" for=\"name_mapel\"\r\n                        >Mata Pelajaran:</label\r\n                      >\r\n                      <div class=\"col-sm-9\">\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          id=\"name_mapel\"\r\n                          placeholder=\"Mata Pelajaran\"\r\n                          v-model=\"form.name_mapel\"\r\n                          required\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"modal-footer\">\r\n                    <button\r\n                      type=\"button\"\r\n                      class=\"btn btn-secondary\"\r\n                      data-dismiss=\"modal\"\r\n                      @click=\"resetForm()\"\r\n                    >\r\n                      Close\r\n                    </button>\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn btn-primary\"\r\n                      :disabled=\"disabled\"\r\n                      v-show=\"!statusmodal\"\r\n                    >\r\n                      <i v-show=\"loading\" class=\"fa fa-spinner fa-spin\"></i>\r\n                      Simpan\r\n                    </button>\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn btn-success\"\r\n                      :disabled=\"disabled\"\r\n                      v-show=\"statusmodal\"\r\n                    >\r\n                      <i v-show=\"loading\" class=\"fa fa-spinner fa-spin\"></i>\r\n                      Ubah\r\n                    </button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n<script>\r\nimport MapelService from \"../../services/mapel.service\";\r\nimport Swal from \"sweetalert2\";\r\nimport Joi from \"joi\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      disabled: false,\r\n      mapels: [],\r\n      statusmodal: false,\r\n      search: \"\",\r\n      form: 