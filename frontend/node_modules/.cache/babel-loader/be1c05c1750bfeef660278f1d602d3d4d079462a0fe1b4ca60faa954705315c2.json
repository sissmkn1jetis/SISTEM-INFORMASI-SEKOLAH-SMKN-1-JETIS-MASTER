{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Dashboard\",\n  data() {\n    return {\n      currentTime: \"\" // Waktu saat ini akan diperbarui secara otomatis\n    };\n  },\n\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    },\n    currentDate() {\n      const now = new Date();\n      const options = {\n        weekday: \"long\",\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\"\n      };\n      return now.toLocaleDateString(\"id-ID\", options);\n    }\n  },\n  mounted() {\n    if (!this.currentUser) {\n      this.$router.push(\"/login\");\n    }\n    // Mulai interval pembaruan jam setiap detik (1000 milidetik)\n    this.startClock();\n  },\n  methods: {\n    startClock() {\n      // Fungsi untuk mengupdate waktu saat ini secara berkala\n      this.updateClock();\n      this.clockInterval = setInterval(this.updateClock, 1000);\n    },\n    updateClock() {\n      // Mengupdate waktu saat ini\n      const now = new Date();\n      const hours = now.getHours().toString().padStart(2, \"0\");\n      const minutes = now.getMinutes().toString().padStart(2, \"0\");\n      const seconds = now.getSeconds().toString().padStart(2, \"0\");\n      this.currentTime = `${hours}:${minutes}:${seconds}`;\n    }\n  },\n  beforeDestroy() {\n    // Membersihkan interval ketika komponen dihancurkan\n    clearInterval(this.clockInterval);\n  }\n};","map":{"version":3,"mappings":";AAsGA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE,CAAE;IACnB,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC,CAAC;IACDC,WAAW,GAAG;MACZ,MAAMC,GAAE,GAAI,IAAIC,IAAI,EAAE;MACtB,MAAMC,OAAM,GAAI;QACdC,OAAO,EAAE,MAAM;QACfC,IAAI,EAAE,SAAS;QACfC,KAAK,EAAE,MAAM;QACbC,GAAG,EAAE;MACP,CAAC;MACD,OAAON,GAAG,CAACO,kBAAkB,CAAC,OAAO,EAAEL,OAAO,CAAC;IACjD;EACF,CAAC;EACDM,OAAO,GAAG;IACR,IAAI,CAAC,IAAI,CAACd,WAAW,EAAE;MACrB,IAAI,