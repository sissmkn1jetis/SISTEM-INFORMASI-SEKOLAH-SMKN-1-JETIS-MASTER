\r\n  <div>\r\n    <!-- Content Header (Page header) -->\r\n    <section class=\"content-header\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row mb-2\">\r\n          <div class=\"col-sm-6\">\r\n            <h1>Dashboard</h1>\r\n          </div>\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Dashboard</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Main content -->\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-8\">\r\n            <div v-if=\"currentUser.role === 'admin'\">\r\n              <!-- Konten untuk Admin -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Admin</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  <p class=\"welcome-msg\">\r\n                    Selamat Datang <strong>{{ currentUser.username }}</strong\r\n                    >, Anda Login Sebagai\r\n                    <strong>{{ currentUser.role }}</strong>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else-if=\"currentUser.role === 'guru'\">\r\n              <!-- Konten untuk Guru -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Guru</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  Selamat Datang\r\n                  <strong>{{ currentUser.username }}</strong> Anda Login Sebagai\r\n                  <strong>{{ currentUser.role }}</strong>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else-if=\"currentUser.role === 'walas'\">\r\n              <!-- Konten untuk Wali Kelas -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Wali Kelas</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  Selamat Datang\r\n                  <strong>{{ currentUser.username }}</strong> Anda Login Sebagai\r\n                  <strong>{{ currentUser.role }}</strong>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else>\r\n              <p>Anda tidak memiliki akses ke dashboard ini.</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Dashboard\",\r\n  data() {\r\n    return {\r\n      currentTime: \"\", // Waktu saat ini akan diperbarui secara otomatis\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n    currentDate() {\r\n      const now = new Date();\r\n      const options = {\r\n        weekday: \"long\",\r\n        year: \"numeric\",\r\n        month: \"long\",\r\n        day: \"numeric\",\r\n      };\r\n      return now.toLocaleDateString(\"id-ID\", options);\r\n    },\r\n  },\r\n  mounted() {\r\n    if (!this.currentUser) {\r\n      this.$router.push(\"/login\");\r\n    }\r\n    // Mulai interval pembaruan jam setiap detik (1000 milidetik)\r\n    this.startClock();\r\n  },\r\n  methods: {\r\n    startClock() {\r\n      // Fungsi untuk mengupdate waktu saat ini secara berkala\r\n      this.updateClock();\r\n      this.clockInterval = setInterval(this.updateClock, 1000);\r\n    },\r\n    updateClock() {\r\n      // Mengupdate waktu saat ini\r\n      const now = new Date();\r\n      const hours = now.getHours().toString().padStart(2, \"0\");\r\n      const minutes = now.getMinutes().toString().padStart(2, \"0\");\r\n      const seconds = now.getSeconds().toString().padStart(2, \"0\");\r\n      this.currentTime = `${hours}:${minutes}:${seconds}`;\r\n    },\r\n  },\r\n  beforeDestroy() {\r\n    // Membersihkan interval ketika komponen dihancurkan\r\n    clearInterval(this.clockInterval);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Gaya tambahan untuk membuat tampilan lebih menarik */\r\n.welcome-msg {\r\n  font-size: 1.5em;\r\n  color: #007bff; /* Warna teks biru */\r\n}\r\n\r\n.clock {\r\n  font-size: 3em;\r\n  text-align: center;\r\n  margin-top: 20px;\r\n  animation: fadeIn 1s ease-in-out; /* Animasi muncul */\r\n}\r\n\r\n.time {\r\n  font-weight: bold;\r\n  animation: pulse 1s infinite; /* Efek pulsasi tak terbatas */\r\n}\r\n\r\n.date {\r\n  font-style: italic;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  0% {\r\n    opacity: 0;\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n@keyframes pulse {\r\n  0% {\r\n    transform: scale(1);\r\n  }\r\n  50% {\r\n    transform: scale(1.1); /* Efek perbesaran */\r\n  }\r\n  100% {\r\n    transform: scale(1);\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}