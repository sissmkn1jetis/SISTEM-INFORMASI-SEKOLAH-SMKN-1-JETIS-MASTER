>\r\n              <div class=\"card-header\">\r\n                <div class=\"card-tools\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-success mb-2\"\r\n                    @click=\"showModal\"\r\n                  >\r\n                    Tambah Walas\r\n                  </button>\r\n                </div>\r\n              </div>\r\n              <div class=\"card-body\">\r\n                <table id=\"example2\" class=\"table table-bordered table-hover\">\r\n                  <thead>\r\n                    <tr>\r\n                      <th>No</th>\r\n                      <th>NIP</th>\r\n                      <th>Guru</th>\r\n                      <th>Kelas</th>\r\n                      <th>Semester</th>\r\n                      <th>Status</th>\r\n                      <th>Tahun Ajaran</th>\r\n                      <th>Action</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"(walass, index) in walas\" :key=\"index.id\">\r\n                      <td>{{ index + 1 }}</td>\r\n                      <td>{{ walass.guruId }}</td>\r\n                      <td>{{ walass.kelassId }}</td>\r\n                      <td>{{ walass.thnAjarId }}</td>\r\n                      <td>\r\n                        <a\r\n                          href=\"#\"\r\n                          class=\"badge bg-info mr-2\"\r\n                          @click=\"showModalEdit(walass.id)\"\r\n                          ><i class=\"fas fa-edit\"></i\r\n                        ></a>\r\n                        <a\r\n                          href=\"#\"\r\n                          class=\"badge bg-danger\"\r\n                          @click=\"deleteData(walass.id)\"\r\n                          ><i class=\"fas fa-trash-alt\"></i\r\n                        ></a>\r\n                      </td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"modalmuncul\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"modalmuncul1\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-lg\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"!statusmodal\"\r\n              >\r\n                Tambah Walas\r\n              </h5>\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"statusmodal\"\r\n              >\r\n                Ubah Walas\r\n              </h5>\r\n              <button\r\n                type=\"button\"\r\n                class=\"close\"\r\n                data-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <span aria-hidden=\"true\" @click=\"resetForm()\">&times;</span>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"card card-default\">\r\n                <!-- /.card-header -->\r\n                <!-- form start -->\r\n                <form\r\n                  @submit.prevent=\"statusmodal ? updateData() : simpanData()\"\r\n                >\r\n                  <div class=\"card-body\">\r\n                    <div class=\"col-6\">\r\n                      <div class=\"form-group\">\r\n                        <label>Name</label>\r\n                        <select\r\n                          class=\"form-control select2\"\r\n                          v-model=\"form.guruId\"\r\n                          required\r\n                        >\r\n                          <option disabled value=\"\">-- Pilih --</option>\r\n                          <option\r\n                            v-for=\"guru in gurus\"\r\n                            :key=\"guru.id\"\r\n                            :value=\"guru.id\"\r\n                          >\r\n                            {{ guru.name }}\r\n                          </option>\r\n                        </select>\r\n                      </div>\r\n\r\n                      <div class=\"form-group\">\r\n                        <label>NIP</label>\r\n                        <select\r\n                          class=\"form-control select2\"\r\n                          v-model=\"form.guruId\"\r\n                          disabled\r\n                        >\r\n                          <!-- <option disabled value=\"\">-- Pilih --</option> -->\r\n                          <option\r\n                            v-for=\"guru in gurus\"\r\n                            :key=\"guru.id\"\r\n                            :value=\"guru.id\"\r\n                          >\r\n                            {{ guru.nip }}\r\n                          </option>\r\n                        </select>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"modal-footer\">\r\n                    <button\r\n                      type=\"button\"\r\n                      class=\"btn btn-secondary\"\r\n                      data-dismiss=\"modal\"\r\n                      @click=\"resetForm()\"\r\n                    >\r\n                      Close\r\n                    </button>\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn btn-primary\"\r\n                      :disabled=\"disabled\"\r\n                      v-show=\"!statusmodal\"\r\n                    >\r\n                      <i v-show=\"loading\" class=\"fa fa-spinner fa-spin\"></i>\r\n                      Simpan\r\n                    </button>