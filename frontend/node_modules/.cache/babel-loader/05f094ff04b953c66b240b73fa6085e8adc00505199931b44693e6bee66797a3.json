;\n      }).catch(e => {\n        console.log(e);\n      });\n\n      // UserService.getProfileWalas()\n      //   .then((response) => {\n      //     this.filterguru = response.data.filterdataguru;\n      //     console.log(response.data.filterdataguru);\n      //   })\n      //   .catch((e) => {\n      //     console.log(e);\n      //   });\n    },\n\n    // onMapelSelect() {\n    //   console.log(\"Selected Mapel:\", this.selectedMapel);\n\n    //   if (this.selectedMapel && this.selectedMapel.kelas) {\n    //     console.log(\"Kelas Data:\", this.selectedMapel.kelas);\n\n    //     if (this.selectedMapel.kelas.siswa) {\n    //       console.log(\"Siswa Data:\", this.selectedMapel.kelas.siswa);\n    //     } else {\n    //       console.log(\"No Siswa Data Found\");\n    //     }\n    //   }\n    // },\n\n    getNilaiNK(siswaId, jenis, i) {\n      const nilai = this.nkData.find(item => item.siswaId === siswaId);\n      if (nilai) {\n        return nilai[jenis + i] || \"-\";\n      }\n      return \"-\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAwLA,OAAOA,WAAU,MAAO,6BAA6B;AACrD,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,CAAC,CAAC;MACTC,aAAa,EAAE,IAAI;MACnBC,YAAY,EAAE,EAAE;MAChBC,UAAU,EAAE,EAAE;MACdC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,GAAG;IACRhB,WAAW,CAACiB,eAAe,EAAE,CAACC,IAAI,CAC/BC,QAAQ,IAAK;MACZ,IAAI,CAACjB,OAAM,GAAIiB,QAAQ,CAAClB,IAAI;IAC9B,CAAC,EACAmB,KAAK,IAAK;MACT,IAAI,CAAClB,OAAM,GACRkB,KAAK,CAACD,QAAO,IACZC,KAAK,CAACD,QAAQ,CAAClB,IAAG,IAClBmB,KAAK,CAACD,QAAQ,CAAClB,IAAI,CAACoB,OAAO,IAC7BD,KAAK,CAACC,OAAM,IACZD,KAAK,CAACE,QAAQ,EAAE;IACpB,EACD;EACH,CAAC;EACDC,OAAO,EAAE;IACPR,OAAO,GAAG;MACRf,WAAW,CAACiB,eAAe,EAAC,CACzBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAAChB,KAAI,GAAIgB,QAAQ,CAAClB,IAAI,CAACY,IAAI;QAC/B,IAAI,CAACN,MAAK,GAAIY,QAAQ,CAAClB,IAAI,CAACuB,EAAE;QAC9B,IA