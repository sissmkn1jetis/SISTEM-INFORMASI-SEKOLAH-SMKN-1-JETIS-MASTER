en","response","catch","e","console","log","getMapel","getKelas","finish","simpanData","createUser","from","Toast","mixin","toast","position","showConfirmButton","timer","timerProgressBar","didOpen","addEventListener","stopTimer","resumeTimer","fire","icon","title","fail","updateData","updateUser","deleteData","text","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","value","deleteUser"],"sourceRoot":"","sources":["D:\\filee\\TA APP MODUL PENILAIAN SISWA\\frontend\\src\\components\\DataUser\\Userlist.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <section class=\"content-header\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row mb-2\">\r\n          <div class=\"col-sm-6\">\r\n            <h1>Data User</h1>\r\n          </div>\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Data User</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <div class=\"card\">\r\n              <div class=\"card-header\">\r\n                <div class=\"card-tools\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-success mb-2\"\r\n                    @click=\"showModal\"\r\n                  >\r\n                    Tambah User\r\n                  </button>\r\n                </div>\r\n              </div>\r\n              <div class=\"card-body\">\r\n                <div class=\"table-responsive\">\r\n                  <table id=\"example2\" class=\"table table-bordered table-hover\">\r\n                    <thead>\r\n                      <tr>\r\n                        <th class=\"text-center\">No</th>\r\n                        <th class=\"text-center\">Username</th>\r\n                        <th class=\"text-center\">Name</th>\r\n                        <th class=\"text-center\">Email</th>\r\n                        <th class=\"text-center\">Role</th>\r\n                        <th class=\"text-center\">NIP</th>\r\n                        <th class=\"text-center\">Tanggal Lahir</th>\r\n                        <th class=\"text-center\">Jenis Kelamin</th>\r\n                        <th class=\"text-center\">Alamat</th>\r\n                        <th class=\"text-center\">Mengajar</th>\r\n                        <th class=\"text-center\">Kelas</th>\r\n                        <th class=\"text-center\">Action</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"(user, index) in users\" :key=\"index.id\">\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ index + 1 }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ user.username }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ user.name }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ user.email }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'admin'\"\r\n                        >\r\n                          <span class=\"badge bg-danger\">{{ user.role }}</span>\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          <span class=\"badge bg-warning\">{{ user.role }}</span>\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'walas'\"\r\n                        >\r\n                          <span class=\"badge bg-success\">{{ user.role }}</span>\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'admin'\"\r\n                        >\r\n                          -\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ user.tgl_lahir }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">{{ user.jkg }}</td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ user.alamat }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"\r\n                            (user && user.role === 'walas') ||\r\n                            user.role === 'guru'\r\n                          \"\r\n                        >\r\n                          {{ user.mapel.name_mapel }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"\r\n                            (user && user.role === 'walas') ||\r\n                            user.role === 'guru'\r\n                          \"\r\n                        >\r\n                          {{ user.kelas.name_kelas }}\r\n                        </td>\r\n                        <td\r\n                         