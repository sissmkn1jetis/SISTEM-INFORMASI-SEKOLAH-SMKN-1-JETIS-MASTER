ired\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\" v-if=\"guru\">\r\n                      <label>Mengajar</label>\r\n                      <select\r\n                        class=\"form-control select2\"\r\n                        v-model=\"form.mapelId\"\r\n                        required\r\n                      >\r\n                        <option disabled value=\"\">-- Pilih --</option>\r\n                        <option\r\n                          v-for=\"mapel in mapels\"\r\n                          :key=\"mapel.id\"\r\n                          :value=\"mapel.id\"\r\n                        >\r\n                          {{ mapel.name_mapel }}\r\n                        </option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"form-group\" v-if=\"walas\">\r\n                      <label>Kelas</label>\r\n                      <select\r\n                        class=\"form-control select2\"\r\n                        v-model=\"form.kelasId\"\r\n                        required\r\n                      >\r\n                        <option disabled value=\"\">-- Pilih --</option>\r\n                        <option\r\n                          v-for=\"kelass in kelas\"\r\n                          :key=\"kelass.id\"\r\n                          :value=\"kelass.id\"\r\n                        >\r\n                          {{ kelass.name_kelas }}\r\n                        </option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"modal-footer\">\r\n                    <button\r\n                      type=\"button\"\r\n                      class=\"btn btn-secondary\"\r\n                      data-dismiss=\"modal\"\r\n                      @click=\"resetForm()\"\r\n                    >\r\n                      Close\r\n                    </button>\r\n                 