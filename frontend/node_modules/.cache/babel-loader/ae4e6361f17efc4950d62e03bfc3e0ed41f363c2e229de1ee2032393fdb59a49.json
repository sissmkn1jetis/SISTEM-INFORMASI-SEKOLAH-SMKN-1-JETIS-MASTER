{"ast":null,"code":"import axios from \"axios\";\nimport UserService from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      content: null,\n      users: {},\n      selectedMapel: null,\n      statusmodal: false,\n      filteredGuru: [],\n      filterguru: [],\n      nkData: []\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getData();\n  },\n  mounted() {\n    UserService.getProfileWalas().then(response => {\n      this.content = response.data;\n    }, error => {\n      this.content = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  },\n  methods: {\n    showModal() {\n      this.statusmodal = false;\n      $(\"#modalmuncul\").modal(\"show\");\n    },\n    getData() {\n      UserService.getProfileWalas().then(response => {\n        this.users = response.data.user;\n        this.nkData = response.data.nk;\n        this.filterguru = response.data.filterdataguru;\n        const kelasId = this.users.wala.kelas.id;\n        this.filteredGuru = this.filterguru.filter(guru => {\n          return guru.kelas[0].id === kelasId;\n        });\n        console.log(\"Users Data:\", this.users);\n        console.log(\"Filter Guru Data:\", this.filterguru);\n        console.log(\"Filtered Guru Data:\", this.filteredGuru);\n        console.log(\"NK Data:\", this.nkData);\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    // cetakPDFNK() {\n    //   const requestData = {\n    //     selectedMapel: this.selectedMapel,\n    //   };\n\n    //   UserService.CetakPDFNK(requestData, {\n    //     withCredentials: true,\n    //     responseType: \"blob\",\n    //   })\n    //     .then((response) => {\n    //       const namaMapel = this.selectedMapel.mapel.name_mapel;\n    //       const filename = `${namaMapel}_nilai_ketrampilan.pdf`;\n\n    //       const blob = new Blob([response.data], {\n    //         type: \"application/pdf\",\n    //       });\n    //       const url = window.URL.createObjectURL(blob);\n    //       const link = document.createElement(\"a\");\n    //       link.href = url;\n    //       link.setAttribute(\"download\", filename);\n    //       document.body.appendChild(link);\n    //       link.click();\n    //       window.URL.revokeObjectURL(url);\n    //     })\n    //     .catch((error) => {\n    //       console.log(\"Gagal mencetak PDF:\", error);\n    //     });\n    // },\n\n    async cetakPDFNK() {\n      try {\n        const mapeldanguru = this.filteredGuru ? this.filteredGuru.map(guru => `${guru.mapel.name_mapel} - ${guru.nama}`).join(\", \") : \"\";\n        const tahunajran = this.filteredGuru ? this.filteredGuru.map(guru => `${guru.thn_ajar.thn_ajaran}`).join(\", \") : \"\";\n        const htmlContent = `\n          <!DOCTYPE html>\n            <html>\n            <head>\n            <meta charset=\"UTF-8\">\n            <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n            <title>Data Nilai Ketrampilan</title>\n            <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css\" rel=\"stylesheet\">\n            <style>\n            table {\n                width: 100%;\n                border-collapse: collapse;\n            }\n            th, td {\n                border: 1px solid #dddddd;\n                padding: 8px;\n                text-align: center;\n                font-size: 12px;\n            }\n            th {\n                background-color: #f2f2f2;\n            }\n            @page {\n                size: A4;\n                margin: 20px;\n            }\n            .container {\n                margin: 0;\n                padding: 0;\n            }\n            .display-4 {\n                text-align: center;\n            }\n            .label {\n                display: inline-block;\n                width: 150px; \n                text-align: left; \n                font-weight: bold; \n            }\n        </style>\n            </head>\n            <body>\n            <div class=\"container\">\n                  <h1 class=\"display-4\">Data Nilai Ketrampilan</h1>\n                  <p class=\"lead\"><span class=\"label\">NIP</span>: ${this.users.wala.guru.nip}</p>\n                  <p class=\"lead\"><span class=\"label\">Nama</span>: ${this.users.wala.guru.nama}</p>\n                  <p class=\"lead\"><span class=\"label\">Wali Kelas</span>: ${this.users.wala.kelas.name_kelas}</p>\n                  <p class=\"lead\"><span class=\"label\">Mata Pelajaran</span>: ${mapeldanguru}</p>\n                  <p class=\"lead\"><span class=\"label\">Tahun Pelajaran</span>: ${tahunajran}</p>\n                <table class=\"table table-bordered table-striped\">\n                <thead>\n            <tr>\n              <th colSpan=\"32\" class=\"text-center\">\n                Nilai Ketrampilan\n              </th>\n            </tr>\n            <tr>\n              <th rowSpan=\"2\" class=\"align-middle\">No</th>\n              <th rowSpan=\"2\" class=\"align-middle text-center\">\n                NIS\n              </th>\n              <th rowSpan=\"2\" class=\"align-middle text-center\">\n                Siswa\n              </th>\n              <th colSpan=\"8\" class=\"text-center\">Proses</th>\n              <th colSpan=\"8\" class=\"text-center\">Proyek</th>\n              <th colSpan=\"8\" class=\"text-center\">Produk</th>\n            </tr>\n            <tr>\n              <th class=\"text-center\">1</th>\n              <th class=\"text-center\">2</th>\n              <th class=\"text-center\">3</th>\n              <th class=\"text-center\">4</th>\n              <th class=\"text-center\">5</th>\n              <th class=\"text-center\">6</th>\n              <th class=\"text-center\">7</th>\n              <th class=\"text-center\">8</th>\n              <th class=\"text-center\">1</th>\n              <th class=\"text-center\">2</th>\n              <th class=\"text-center\">3</th>\n              <th class=\"text-center\">4</th>\n              <th class=\"text-center\">5</th>\n              <th class=\"text-center\">6</th>\n              <th class=\"text-center\">7</th>\n              <th class=\"text-center\">8</th>\n              <th class=\"text-center\">1</th>\n              <th class=\"text-center\">2</th>\n              <th class=\"text-center\">3</th>\n              <th class=\"text-center\">4</th>\n              <th class=\"text-center\">5</th>\n              <th class=\"text-center\">6</th>\n              <th class=\"text-center\">7</th>\n              <th class=\"text-center\">8</th>\n            </tr>\n          </thead>\n                <tbody>\n                       ${this.filteredGuru.map(guru => guru.kelas[0].siswa.map((siswa, index) => `\n                  <tr>\n                    <td class=\"align-middle text-center\">${index + 1}</td>\n                    <td class=\"align-middle text-center\">${siswa.nis}</td>\n                    <td class=\"align-middle text-center\">${siswa.name}</td>\n                    ${[...Array(8).keys()].map(i => `\n                      <td class=\"align-middle text-center\">\n                        ${this.getNilaiNK(siswa.id, guru.mapel.id, \"proses\", i + 1)}\n                      </td>`).join(\"\")}\n                    ${[...Array(8).keys()].map(i => `\n                      <td class=\"align-middle text-center\">\n                        ${this.getNilaiNK(siswa.id, guru.mapel.id, \"proyek\", i + 1)}\n                      </td>`).join(\"\")}\n                    ${[...Array(8).keys()].map(i => `\n                      <td class=\"align-middle text-center\">\n                        ${this.getNilaiNK(siswa.id, guru.mapel.id, \"produk\", i + 1)}\n                      </td>`).join(\"\")}\n                  </tr>\n                `)).join(\"\")}\n                </tbody>\n            </table>\n            </div>\n            </body>\n            </html>\n          `;\n        const response = await axios.post(\"http://localhost:8080/api/pdfnilaiketrampilan\", {\n          htmlContent: htmlContent\n        }, {\n          responseType: \"blob\",\n          withCredentials: true\n        });\n        const pdfUrl = URL.createObjectURL(new Blob([response.data], {\n          type: \"application/pdf\"\n        }));\n        const namaMapel = this.selectedMapel.mapel.name_mapel;\n        const filename = `${namaMapel}_nilai_ketrampilan.pdf`;\n\n        // Membuat elemen <a> untuk menautkan dan mengunduh file PDF\n        const link = document.createElement(\"a\");\n        link.href = pdfUrl;\n        link.download = filename;\n        document.body.appendChild(link);\n        link.click();\n        document.body.removeChild(link);\n      } catch (error) {\n        alert(\"error data\");\n        console.error(\"Error generating PDF:\", error);\n      }\n    },\n    // async cetakPDFNK() {\n    //   try {\n    //     const requestData = {\n    //       selectedMapel: this.selectedMapel,\n    //     };\n\n    //     // Replace with the correct URL to your PDF generation endpoint\n    //     const apiUrl = \"http://localhost:8080/api/pdfnilaiketrampilan\";\n\n    //     const response = await axios.post(apiUrl, requestData, {\n    //       responseType: \"blob\",\n    //       withCredentials: true, // Include cookies if needed for authentication\n    //     });\n\n    //     const blob = new Blob([response.data], {\n    //       type: \"application/pdf\",\n    //     });\n\n    //     const namaMapel = this.selectedMapel.mapel.name_mapel;\n    //     const filename = `${namaMapel}_nilai_ketrampilan.pdf`;\n\n    //     const url = window.URL.createObjectURL(blob);\n\n    //     const link = document.createElement(\"a\");\n    //     link.href = url;\n    //     link.setAttribute(\"download\", filename);\n    //     document.body.appendChild(link);\n\n    //     link.click();\n\n    //     window.URL.revokeObjectURL(url);\n\n    //     alert(\"PDF berhasil dicetak\");\n    //   } catch (error) {\n    //     console.error(\"Gagal mencetak PDF:\", error);\n    //     alert(\"Gagal mencetak PDF. Silakan coba lagi nanti.\");\n    //   }\n    // },\n\n    getNilaiNK(siswaId, guruId, jenis, i) {\n      const nilai = this.nkData.find(item => item.siswaId === siswaId && item.guruId === guruId);\n      if (nilai) {\n        return nilai[jenis + i] || \"-\";\n      }\n      return \"-\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAuaA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,WAAU,MAAO,6BAA6B;AACrD,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,CAAC,CAAC;MACTC,aAAa,EAAE,IAAI;MACnBC,WAAW,EAAE,KAAK;MAClBC,YAAY,EAAE,EAAE;MAChBC,UAAU,EAAE,EAAE;MACdC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,GAAG;IACRjB,WAAW,CAACkB,eAAe,EAAE,CAACC,IAAI,CAC/BC,QAAQ,IAAK;MACZ,IAAI,CAAClB,OAAM,GAAIkB,QAAQ,CAACnB,IAAI;IAC9B,CAAC,EACAoB,KAAK,IAAK;MACT,IAAI,CAACnB,OAAM,GACRmB,KAAK,CAACD,QAAO,IACZC,KAAK,CAACD,QAAQ,CAACnB,IAAG,IAClBoB,KAAK,CAACD,QAAQ,CAACnB,IAAI,CAACqB,OAAO,IAC7BD,KAAK,CAACC,OAAM,IACZD,KAAK,CAACE,QAAQ,EAAE;IACpB,EACD;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,IAAI,CAACpB,WAAU,GAAI,KAAK;MACxBqB,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;IACjC,CAAC;IACDX,OAAO,GAAG;MACRhB,WAAW,CAACkB,eAAe,EAAC,CACzBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACjB,KAAI,GAAIiB,QAAQ,CAACnB,IAAI,CAACa,IAAI;QAC/B,IAAI,CAACN,MAAK,GAAIY,QAAQ,CAACnB,IAAI,CAAC2B,EAAE;QAC9B,IAAI,CAACrB,UAAS,GAAIa,QAAQ,CAACnB,IAAI,CAAC4B,cAAc;QAC9C,MAAMC,OAAM,GAAI,IAAI,CAAC3B,KAAK,CAAC4B,IAAI,CAACC,KAAK,CAACC,EAAE;QACxC,IAAI,CAAC3B,YAAW,GAAI,IAAI,CAACC,UAAU,CAAC2B,MAAM,CAAEC,IAAI,IAAK;UACnD,OAAOA,IAAI,CAACH,KAAK,CAAC,CAAC,CAAC,CAACC,EAAC,KAAMH,OAAO;QACrC,CAAC,CAAC;QAEFM,OAAO,CAACC,GAAG,CAA