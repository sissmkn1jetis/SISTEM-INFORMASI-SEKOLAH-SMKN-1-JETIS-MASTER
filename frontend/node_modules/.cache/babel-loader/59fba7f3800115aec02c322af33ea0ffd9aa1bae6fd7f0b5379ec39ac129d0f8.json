{"ast":null,"code":"import UserService from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      content: null,\n      users: [],\n      selectedMapel: \"\",\n      nkData: [],\n      selectedSiswaData: []\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getData();\n  },\n  mounted() {\n    UserService.getProfileWalas().then(response => {\n      this.content = response.data;\n    }, error => {\n      this.content = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  },\n  methods: {\n    getData() {\n      UserService.getProfileWalas().then(response => {\n        this.users = response.data;\n        this.nkData = response.data.nk;\n        console.log(response.data);\n        if (response.data.filterdataguru.length > 0) {\n          this.selectedSiswaData = response.data.filterdataguru[0].kelas.siswa;\n        }\n      }).catch(e => {\n        console.log(e);\n      });\n      UserService.getProfileWalas().then(response => {\n        this.content = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    onMapelChange() {\n      const selectedMapelId = this.selectedMapel;\n\n      // Cari data siswa yang sesuai dengan mata pelajaran yang dipilih\n      const selectedGuru = this.users.filterdataguru.find(guru => guru.mapel.id === selectedMapelId);\n      if (selectedGuru) {\n        this.selectedSiswaData = selectedGuru.kelas.siswa;\n        console.log(selectedGuru);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AA+MA,OAAOA,WAAU,MAAO,6BAA6B;AACrD,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE,EAAE;MACjBC,MAAM,EAAE,EAAE;MACVC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EACh