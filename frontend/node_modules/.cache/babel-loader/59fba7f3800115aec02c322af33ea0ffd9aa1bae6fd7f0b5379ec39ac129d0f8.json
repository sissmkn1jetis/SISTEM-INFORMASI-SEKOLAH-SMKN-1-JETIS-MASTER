  <section class=\"content-header\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row mb-2\">\r\n          <div class=\"col-sm-6\">\r\n            <h1>Data Nilai Ketrampilan</h1>\r\n          </div>\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Data Nilai Ketrampilan</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <div class=\"card\">\r\n              <div class=\"card-header\">\r\n                <h3 class=\"card-title\"></h3>\r\n              </div>\r\n              <form>\r\n                <div class=\"card-body\">\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"nip\"\r\n                      >NIP:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users.user\">\r\n                        {{ users.user.wala.guru.nip }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"nama\"\r\n                      >Nama:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users.user\">\r\n                        {{ users.user.wala.guru.nama }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"kelas\"\r\n                      >Kelas:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users.user\">\r\n                        {{ users.user.wala.kelas.name_kelas }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"mapel\"\r\n                      >Pilih Mata Pelajaran:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <select\r\n                        class=\"form-control select2\"\r\n                        v-model=\"selectedMapel\"\r\n                        @change=\"onMapelChange\"\r\n                      >\r\n                        <option\r\n                          v-for=\"tes in users.filterdataguru\"\r\n                          :key=\"tes.id\"\r\n                          :value=\"tes\"\r\n                        >\r\n                          {{ tes.mapel.name_mapel }} - {{ tes.nama }}\r\n                        </option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n\r\n              <div class=\"card-body\">\r\n                <div class=\"table-responsive\">\r\n                  <table id=\"myTable\" class=\"table table-bordered table-hover\">\r\n                    <thead>\r\n                      <tr>\r\n                        <th colSpan=\"32\" class=\"text-center\">\r\n                          Nilai Ketrampilan\r\n                        </th>\r\n                      </tr>\r\n                      <tr>\r\n                        <th rowSpan=\"2\" class=\"align-middle\">No</th>\r\n                        <th rowSpan=\"2\" class=\"align-middle text-center\">\r\n                          NIS\r\n                        </th>\r\n                        <th rowSpan=\"2\" class=\"align-middle text-center\">\r\n                          Siswa\r\n                        </th>\r\n                        <th colSpan=\"8\" class=\"text-center\">Proses</th>\r\n                        <th colSpan=\"8\" class=\"text-center\">Proyek</th>\r\n                        <th colSpan=\"8\" class=\"text-center\">Produk</th>\r\n                      </tr>\r\n                      <tr>\r\n                        <th class=\"text-center\">1</th>\r\n                        <th class=\"text-center\">2</th>\r\n                        <th class=\"text-center\">3</th>\r\n                        <th class=\"text-center\">4</th>\r\n                        <th class=\"text-center\">5</th>\r\n                        <th class=\"text-center\">6</th>\r\n                        <th class=\"text-center\">7</th>\r\n                        <th class=\"text-center\">8</th>\r\n                        <th class=\"text-center\">1</th>\r\n                        <th class=\"text-center\">2</th>\r\n                        <th class=\"text-center\">3</th>\r\n                        <th class=\"text-center\">4</th>\r\n                        <th class=\"text-center\">5</th>\r\n                        <th class=\"text-center\">6</th>\r\n                        <th class=\"text-center\">7</th>\r\n                        <th class=\"text-center\">8</th>\r\n                        <th class=\"text-center\">1</th>\r\n                        <th class=\"text-center\">2</th>\r\n                        <th class=\"text-center\">3</th>\r\n                        <th class=\"text-center\">4</th>\r\n                        <th class=\"text-center\">5</th>\r\n                        <th class=\"text-center\">6</th>\r\n                        <th class=\"text-center\">7</th>\r\n                        <th class=\"text-center\">8</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr\r\n                        v-for=\"(child, index) in selectedSiswaData\"\r\n                        :key=\"index.id\"\r\n                      >\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ index + 1 }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ child.nis }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ child.name }}\r\n                        </td>\r\n                        <td\r\n                          v-for=\"i in 8\"\r\n                          :key=\"i\"\r\n                          class=\"align-middle text-center\"\r\n                        >\r\n                          <div v-if=\"nkData.length > 0\">\r\n                            {{\r\n                              nkData.find((item) => item.siswaId === child.id)\r\n                                ? nkData.find(\r\n                                    (item) => item.siswaId === child.id\r\n                                  )[\"proses\" + i]\r\n                                : \"-\"\r\n                            }}\r\n                          </div>\r\n                          <div v-else>-</div>\r\n                        </td>\r\n                        <td\r\n                          v-for=\"i in 8\"\r\n                          :key=\"i\"\r\n                          class=\"align-middle text-center\"\r\n                        >\r\n                          <div v-if=\"nkData.length > 0\">\r\n                            {{\r\n                              nkData.find((item) => item.siswaId === child.id)\r\n                                ? nkData.find(\r\n                                    (item) => item.siswaId === child.id\r\n                                  )[\"proyek\" + i]\r\n                                : \"-\"\r\n                            }}\r\n                          </div>\r\n                          <div v-else>-</div>\r\n                        </td>\r\n                        <td\r\n                          v-for=\"i in 8\"\r\n                          :key=\"i\"\r\n                          class=\"align-middle text-center\"\r\n                        >\r\n                          <div v-if=\"nkData.length > 0\">\r\n                            {{\r\n                              nkData.find((item) => item.siswaId === child.id)\r\n                                ? nkData.find(\r\n                                    (item) => item.siswaId === child.id\r\n                                  )[\"produk\" + i]\r\n                                : \"-\"\r\n                            }}\r\n                          </div>\r\n                          <div v-else>-</div>\r\n                        </td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from \"../../services/user.service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      content: null,\r\n      users: [],\r\n      selectedMapel: \"\",\r\n      nkData: [],\r\n      selectedSiswaData: [],\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n  mounted() {\r\n    UserService.getProfileWalas().then(\r\n      (response) => {\r\n        this.content = response.data;\r\n      },\r\n      (error) => {\r\n        this.content =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n  methods: {\r\n    getData() {\r\n      UserService.getProfileWalas()\r\n        .then((response) => {\r\n          this.users = response.data;\r\n          this.nkData = response.data.nk;\r\n          console.log(response.data);\r\n\r\n          if (response.data.filterdataguru.length > 0) {\r\n            this.selectedSiswaData =\r\n              response.data.filterdataguru[0].kelas.siswa;\r\n          }\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n\r\n      UserService.getProfileWalas()\r\n        .then((response) => {\r\n          this.content = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    onMapelChange() {\r\n      const selectedMapelId = this.selectedMapel;\r\n\r\n      // Cari data siswa yang sesuai dengan mata pelajaran yang dipilih\r\n      const selectedGuru = this.users.filterdataguru.find(\r\n        (guru) => guru.mapel.id === selectedMapelId\r\n      );\r\n\r\n      if (selectedGuru) {\r\n        this.selectedSiswaData = selectedGuru.kelas.siswa;\r\n        console.log(selectedGuru);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}