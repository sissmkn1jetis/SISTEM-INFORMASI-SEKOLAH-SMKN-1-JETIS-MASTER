{"ast":null,"code":"import AbsensiService from \"../../services/absensi.service\";\nimport SiswaService from \"../../services/siswa.service\";\nimport Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      loading: false,\n      disabled: false,\n      siswas: {},\n      absensi: {},\n      statusmodal: false,\n      form: {\n        siswaId: \"\",\n        lokasi_masuk: \"\",\n        lokasi_pulang: \"\",\n        tgl_absen: \"\",\n        jm_masuk: \"\",\n        jm_pulang: \"\",\n        foto_masuk: \"\",\n        foto_pulang: \"\"\n      }\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getdata();\n  },\n  methods: {\n    showModal() {\n      this.statusmodal = false;\n      this.resetForm();\n      $(\"#modalmuncul\").modal(\"show\");\n    },\n    showModalEdit(absen) {\n      this.statusmodal = true;\n      $(\"#modalmuncul\").modal(\"show\");\n      this.getSiswaById(absen);\n    },\n    getdata() {\n      this.$Progress.start();\n      AbsensiService.getAbsensi().then(response => {\n        this.absensi = response.data;\n        $(function () {\n          $(\"#myTable\").DataTable({\n            responsive: false,\n            lengthChange: true,\n            autoWidth: true,\n            buttons: [\"copy\", \"csv\", \"excel\", \"pdf\", \"print\"]\n          }).buttons().container().appendTo(\"#myTable_wrapper .col-md-6:eq(0)\");\n        });\n      }).catch(e => {\n        console.log(e);\n      });\n      SiswaService.getSiswa().then(response => {\n        this.siswas = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n      this.$Progress.finish();\n    },\n    getAbsensiById(id) {\n      AbsensiService.getAbsensiById(id).then(response => {\n        this.form = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    simpanData() {\n      this.$Progress.start();\n      this.loading = true;\n      this.disabled = true;\n      AbsensiService.createAbsensi(this.form).then(response => {\n        this.from = response.data;\n        console.log(response.data);\n        this.getdata();\n        this.resetForm();\n        $(\"#modalmuncul\").modal(\"hide\");\n        const Toast = Swal.mixin({\n          toast: true,\n          position: \"top-end\",\n          showConfirmButton: false,\n          timer: 3000,\n          timerProgressBar: true,\n          didOpen: toast => {\n            toast.addEventListener(\"mouseenter\", Swal.stopTimer);\n            toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n          }\n        });\n        Toast.fire({\n          icon: \"success\",\n          title: \"Data Berhasil Tersimpan\"\n        });\n        this.$Progress.finish();\n        this.loading = false;\n        this.disabled = false;\n      }).catch(e => {\n        this.$Progress.fail();\n        this.loading = false;\n        this.disabled = false;\n        console.log(e);\n      });\n    },\n    updateData() {\n      this.$Progress.start();\n      this.loading = true;\n      this.disabled = true;\n      AbsensiService.updateAbsensi(this.form.id, this.form).then(response => {\n        this.from = response.data;\n        console.log(response.data);\n        this.getdata();\n        this.resetForm();\n        $(\"#modalmuncul\").modal(\"hide\");\n        const Toast = Swal.mixin({\n          toast: true,\n          position: \"top-end\",\n          showConfirmButton: false,\n          timer: 3000,\n          timerProgressBar: true,\n          didOpen: toast => {\n            toast.addEventListener(\"mouseenter\", Swal.stopTimer);\n            toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n          }\n        });\n        Toast.fire({\n          icon: \"success\",\n          title: \"Data Berhasil Terupdate\"\n        });\n        this.$Progress.finish();\n        this.loading = false;\n        this.disabled = false;\n      }).catch(e => {\n        this.$Progress.fail();\n        this.loading = false;\n        this.disabled = false;\n        console.log(e);\n      });\n    },\n    deleteData(id) {\n      Swal.fire({\n        title: \"Anda Yakin Ingin Menghapus Data Ini ?\",\n        text: \"Klik Batal Untuk Membatalkan Penghapusan\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Hapus\"\n      }).then(result => {\n        if (result.value) {\n          AbsensiService.deleteAbsensi(id).then(response => {\n            Swal.fire(\"Terhapus\", \"Data Anda Sudah Terhapus\", \"success\");\n            console.log(response.data);\n            this.getdata();\n          }).catch(() => {\n            Swal.fire(\"Gagal\", \"Data Gagal Terhapus\", \"warning\");\n          });\n        }\n      });\n    },\n    resetForm() {\n      this.form.siswaId = \"\", this.form.lokasi_masuk = \"\", this.form.lokasi_pulang = \"\", this.form.tgl_absen = \"\", this.form.jm_masuk = \"\", this.form.jm_pulang = \"\", this.form.foto_masuk = \"\", this.form.foto_pulang = \"\";\n    }\n  }\n};","map":{"version":3,"mappings":"AA2MA,OAAOA,cAAa,MAAO,gCAAgC;AAC3D,OAAOC,YAAW,MAAO,8BAA8B;AACvD,OAAOC,IAAG,MAAO,aAAa;AAC9B,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,KAAK;MACfC,MAAM,EAAE,CAAC,CAAC;MACVC,OAAO,EAAE,CAAC,CAAC;MACXC,WAAW,EAAE,KAAK;MAClBC,IAAI,EAAE;QACJC,OAAO,EAAE,EAAE;QACXC,YAAY,EAAE,EAAE;QAChBC,aAAa,EAAE,EAAE;QACjBC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,SAAS,EAAE,EAAE;QACbC,UAAU,EAAE,EAAE;QACdC,WAAW,EAAE;MACf;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,IAAI,CAACnB,WAAU,GAAI,KAAK;MACxB,IAAI,CAACoB,SAAS,EAAE;MAChBC,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;IACjC,CAAC;IACDC,aAAa,CAACC,KAAK,EAAE;MACnB,IAAI,CAACxB,WA