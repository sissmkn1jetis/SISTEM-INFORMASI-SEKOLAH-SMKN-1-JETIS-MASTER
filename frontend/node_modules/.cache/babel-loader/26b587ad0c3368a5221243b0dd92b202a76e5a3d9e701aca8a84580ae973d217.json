{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Dashboard\",\n  data() {\n    return {\n      currentHour: \"\",\n      currentMinute: \"\",\n      currentAmPm: \"\"\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  mounted() {\n    if (!this.currentUser) {\n      this.$router.push(\"/login\");\n    }\n    this.startClock();\n  },\n  methods: {\n    startClock() {\n      this.updateClock();\n      setInterval(this.updateClock, 1000);\n    },\n    updateClock() {\n      const now = new Date();\n      let hours = now.getHours();\n      const minutes = now.getMinutes().toString().padStart(2, \"0\");\n      const ampm = hours >= 12 ? \"PM\" : \"AM\";\n      hours = hours % 12 || 12;\n      this.currentHour = hours.toString().padStart(2, \"0\");\n      this.currentMinute = minutes;\n      this.currentAmPm = ampm;\n    }\n  }\n};","map":{"version":3,"mappings":";AAkHA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,EAAE;MACjBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAAC,IAAI,CAACL,WAAW,EAAE;MACrB,IAAI,CAACM,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B;IACA,IAAI,CAACC,UAAU,EAAE;EACnB,CAAC;EACDC,OAAO,EAAE;IACPD,UAAU,GAAG;MACX,IAAI,CAACE,WAAW,EAAE;MAClBC,WAAW,CAAC,IAAI,CAACD,WAAW,EAAE,IAAI,CAAC;IACrC,CAAC;IACDA,WAAW,GAAG;MACZ,MAAME,GAAE,GAAI,IAAIC,IAAI,EAAE;MACtB,IAAIC,KAAI,GAAIF,GAAG,CAACG,QAAQ,EAAE;MAC1B,MAAMC,OAAM,GAAIJ,GAAG,CAACK,UAAU,EAAE,CAACC,QAAQ,EAAE,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAC5D,MAAMC,IAAG,GAAIN,KAAI,IAAK,EAAC,GAAI,IAAG,GAAI,IAAI;MACtCA,KAAI,GAAIA,KAAI,GAAI,EAAC,IAAK,EAAE;MACxB,IAAI,CAAClB,WAAU,GAAIkB,KAAK,CAACI,QAAQ,EAAE,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MACpD,IAAI,CAACtB,aAAY,GAAImB,OAAO;MAC5B,IAAI,CAAClB,WAAU,GAAIsB,IAAI;IACzB;EACF;AACF,CAAC","names":["name","data","currentHour","currentMinute","currentAmPm","computed","currentUser","$store","state","auth","user","mounted","$router","push","startClock","methods","updateClock","setInterval","now","Date","hours","getHours","minutes","getMinutes","toString","padStart","ampm"],"sourceRoot":"","sources":["D:\\filee\\SISTEM-INFORMASI-SEKOLAH-SMKN-1-JETIS-MASTER\\frontend\\src\\components\\Dashboard.vue"],"sourcesContent":["<template>\r\n  <!-- <div class=\"container\">\r\n    <header class=\"jumbotron\">\r\n      <h3>\r\n        <strong>{{ currentUser.username }}</strong> Profile\r\n      </h3>\r\n    </header>\r\n    <p>\r\n      <strong>Token:</strong>\r\n      {{ currentUser.accessToken.substring(0, 20) }} ...\r\n      {{ currentUser.accessToken.substr(currentUser.accessToken.length - 20) }}\r\n    </p>\r\n    <p>\r\n      <strong>Id:</strong>\r\n      {{ currentUser.id }}\r\n    </p>\r\n    <p>\r\n      <strong>Email:</strong>\r\n      {{ currentUser.email }}\r\n    </p>\r\n    <strong>Authorities:</strong>\r\n    <ul>\r\n      <li v-for=\"role in currentUser.roles\" :key=\"role\">{{ role }}</li>\r\n    </ul>\r\n  </div> -->\r\n  <div>\r\n    <!-- Content Header (Page header) -->\r\n    <section class=\"content-header\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row mb-2\">\r\n          <div class=\"col-sm-6\">\r\n            <h1>Dashboard</h1>\r\n          </div>\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Dashboard</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Main content -->\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-8\">\r\n            <div v-if=\"currentUser.role === 'admin'\">\r\n              <!-- Konten untuk Admin -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Admin</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  <p class=\"welcome-msg\">\r\n                    Selamat Datang <strong>{{ currentUser.username }}</strong\r\n                    >, Anda Login Sebagai\r\n                    <strong>{{ currentUser.role }}</strong>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else-if=\"currentUser.role === 'guru'\">\r\n              <!-- Konten untuk Guru -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Guru</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  Selamat Datang\r\n                  <strong>{{ currentUser.username }}</strong> Anda Login Sebagai\r\n                  <strong>{{ currentUser.role }}</strong>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else-if=\"currentUser.role === 'walas'\">\r\n              <!-- Konten untuk Wali Kelas -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Wali Kelas</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  Selamat Datang\r\n                  <strong>{{ currentUser.username }}</strong> Anda Login Sebagai\r\n                  <strong>{{ currentUser.role }}</strong>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else>\r\n              <p>Anda tidak memiliki akses ke dashboard ini.</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-md-4\">\r\n            <!-- Tampilan jam dan tanggal di samping card utama -->\r\n            <div class=\"card\">\r\n              <div class=\"card-body text-center\">\r\n                <div class=\"wall-clock\">\r\n                  <div class=\"hour\">{{ currentHour }}</div>\r\n                  <div class=\"minute\">{{ currentMinute }}</div>\r\n                  <div class=\"ampm\">{{ currentAmPm }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Dashboard\",\r\n  data() {\r\n    return {\r\n      currentHour: \"\",\r\n      currentMinute: \"\",\r\n      currentAmPm: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  mounted() {\r\n    if (!this.currentUser) {\r\n      this.$router.push(\"/login\");\r\n    }\r\n    this.startClock();\r\n  },\r\n  methods: {\r\n    startClock() {\r\n      this.updateClock();\r\n      setInterval(this.updateClock, 1000);\r\n    },\r\n    updateClock() {\r\n      const now = new Date();\r\n      let hours = now.getHours();\r\n      const minutes = now.getMinutes().toString().padStart(2, \"0\");\r\n      const ampm = hours >= 12 ? \"PM\" : \"AM\";\r\n      hours = hours % 12 || 12;\r\n      this.currentHour = hours.toString().padStart(2, \"0\");\r\n      this.currentMinute = minutes;\r\n      this.currentAmPm = ampm;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Gaya tambahan untuk membuat tampilan jam dinding */\r\n.wall-clock {\r\n  font-size: 4em;\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.hour,\r\n.minute,\r\n.ampm {\r\n  display: inline-block;\r\n  margin: 0 10px;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  0% {\r\n    opacity: 0;\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n@keyframes pulse {\r\n  0% {\r\n    transform: scale(1);\r\n  }\r\n  50% {\r\n    transform: scale(1.1);\r\n  }\r\n  100% {\r\n    transform: scale(1);\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}