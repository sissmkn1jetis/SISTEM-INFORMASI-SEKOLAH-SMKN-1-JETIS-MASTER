  <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputPassword1\"\r\n                        placeholder=\"Password\"\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"exampleInputPassword1\"\r\n                        >Confirmasi Password</label\r\n                      >\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputPassword1\"\r\n                        placeholder=\"Confirmasi Password\"\r\n                      />\r\n                    </div>\r\n\r\n                    <div class=\"form-group\">\r\n                      <label>Role</label>\r\n                      <select class=\"form-control\">\r\n                        <option\r\n                          v-for=\"role in user.roles\"\r\n                          :key=\"role.id\"\r\n                          :value=\"role.id\"\r\n                        >\r\n                          {{ role.name }}\r\n                        </option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n            <div class=\"modal-footer\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-secondary\"\r\n                data-dismiss=\"modal\"\r\n              >\r\n                Close\r\n              </button>\r\n              <button class=\"btn btn-primary\">Simpan</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport User from \"../../services/user.service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      users: {},\r\n      roles: {},\r\n    };\r\n  },\r\n  created() {\r\n    this.getdata();\r\n  },\r\n  methods: {\r\n    showModal() {\r\n      $(\"#modalmuncul\").modal(\"show\");\r\n    },\r\n    getdata() {\r\n      User.getUser()\r\n        .then((response) => {\r\n          this.users = response.data;\r\n        })\r\n        .catch((e) => {\r\n          this.users =\r\n            (e.response && e.response.data && e.response.data.message) ||\r\n            e.message ||\r\n            e.toString();\r\n        });\r\n      User.Role()\r\n        .then((response) => {\r\n          this.roles = response.data;\r\n        })\r\n        .catch((e) => {\r\n          this.roles =\r\n            (e.response && e.response.data && e.response.data.message) ||\r\n            e.message ||\r\n            e.toString();\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}