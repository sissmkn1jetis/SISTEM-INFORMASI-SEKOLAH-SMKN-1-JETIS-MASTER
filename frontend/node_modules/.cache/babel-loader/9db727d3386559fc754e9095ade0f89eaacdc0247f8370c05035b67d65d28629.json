{"ast":null,"code":"import IzinService from \"../../services/izin.service\";\nimport SiswaService from \"../../services/siswa.service\";\nimport Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      loading: false,\n      disabled: false,\n      siswas: {},\n      izins: {},\n      statusmodal: false,\n      form: {\n        siswaId: \"\",\n        tgl_izin: \"\",\n        deskripsi: \"\",\n        foto: \"\"\n      }\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getdata();\n  },\n  methods: {\n    showModal() {\n      this.statusmodal = false;\n      this.resetForm();\n      $(\"#modalmuncul\").modal(\"show\");\n    },\n    showModalEdit(izin) {\n      this.statusmodal = true;\n      $(\"#modalmuncul\").modal(\"show\");\n      this.getAbsensiById(izin);\n    },\n    getdata() {\n      this.$Progress.start();\n      IzinService.getIzin().then(response => {\n        this.izins = response.data;\n        $(function () {\n          $(\"#myTable\").DataTable({\n            responsive: false,\n            lengthChange: true,\n            autoWidth: true,\n            buttons: [\"copy\", \"csv\", \"excel\", \"pdf\", \"print\"]\n          }).buttons().container().appendTo(\"#myTable_wrapper .col-md-6:eq(0)\");\n        });\n      }).catch(e => {\n        console.log(e);\n      });\n      SiswaService.getSiswa().then(response => {\n        this.siswas = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n      this.$Progress.finish();\n    },\n    getIzinById(id) {\n      IzinService.getIzinById(id).then(response => {\n        this.form = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    simpanData() {\n      this.$Progress.start();\n      this.loading = true;\n      this.disabled = true;\n      IzinService.createIzin(this.form).then(response => {\n        this.from = response.data;\n        console.log(response.data);\n        this.getdata();\n        this.resetForm();\n        $(\"#modalmuncul\").modal(\"hide\");\n        const Toast = Swal.mixin({\n          toast: true,\n          position: \"top-end\",\n          showConfirmButton: false,\n          timer: 3000,\n          timerProgressBar: true,\n          didOpen: toast => {\n            toast.addEventListener(\"mouseenter\", Swal.stopTimer);\n            toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n          }\n        });\n        Toast.fire({\n          icon: \"success\",\n          title: \"Data Berhasil Tersimpan\"\n        });\n        this.$Progress.finish();\n        this.loading = false;\n        this.disabled = false;\n      }).catch(e => {\n        this.$Progress.fail();\n        this.loading = false;\n        this.disabled = false;\n        console.log(e);\n      });\n    },\n    updateData() {\n      this.$Progress.start();\n      this.loading = true;\n      this.disabled = true;\n      IzinService.updateIzin(this.form.id, this.form).then(response => {\n        this.from = response.data;\n        console.log(response.data);\n        this.getdata();\n        this.resetForm();\n        $(\"#modalmuncul\").modal(\"hide\");\n        const Toast = Swal.mixin({\n          toast: true,\n          position: \"top-end\",\n          showConfirmButton: false,\n          timer: 3000,\n          timerProgressBar: true,\n          didOpen: toast => {\n            toast.addEventListener(\"mouseenter\", Swal.stopTimer);\n            toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n          }\n        });\n        Toast.fire({\n          icon: \"success\",\n          title: \"Data Berhasil Terupdate\"\n        });\n        this.$Progress.finish();\n        this.loading = false;\n        this.disabled = false;\n      }).catch(e => {\n        this.$Progress.fail();\n        this.loading = false;\n        this.disabled = false;\n        console.log(e);\n      });\n    },\n    deleteData(id) {\n      Swal.fire({\n        title: \"Anda Yakin Ingin Menghapus Data Ini ?\",\n        text: \"Klik Batal Untuk Membatalkan Penghapusan\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Hapus\"\n      }).then(result => {\n        if (result.value) {\n          IzinService.deleteIzin(id).then(response => {\n            Swal.fire(\"Terhapus\", \"Data Anda Sudah Terhapus\", \"success\");\n            console.log(response.data);\n            this.getdata();\n          }).catch(() => {\n            Swal.fire(\"Gagal\", \"Data Gagal Terhapus\", \"warning\");\n          });\n        }\n      });\n    },\n    resetForm() {\n      this.form.siswaId = \"\", this.form.tgl_jurnal = \"\", this.form.jam_jurnal = \"\", this.form.isi_jurnal = \"\";\n    }\n  }\n};","map":{"version":3,"mappings":"AA2LA,OAAOA,WAAU,MAAO,6BAA6B;AACrD,OAAOC,YAAW,MAAO,8BAA8B;AACvD,OAAOC,IAAG,MAAO,aAAa;AAC9B,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,KAAK;MACfC,MAAM,EAAE,CAAC,CAAC;MACVC,KAAK,EAAE,CAAC,CAAC;MACTC,WAAW,EAAE,KAAK;MAClBC,IAAI,EAAE;QACJC,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,SAAS,EAAE,EAAE;QACbC,IAAI,EAAE;MACR;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,IAAI,CAACf,WAAU,GAAI,KAAK;MACxB,IAAI,CAACgB,SAAS,EAAE;MAChBC,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;IACjC,CAAC;IACDC,aAAa,CAACC,IAAI,EAAE;MAClB,IAAI,CAACpB,WAAU,GAAI,IAAI;MACvBiB,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;MAC/B,IAAI,CAACG,cAAc,CAACD,IAAI,CAAC;IAC3B,CAAC;IACDP,OAAO,GAAG;MACR,IAAI,CAACS,SAAS,CAACC,KAAK,EAAE;MACtB/B,WAAW,CAACgC,OAAO,EAAC,CACjBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAAC3B,KAAI,GAAI2B,QAAQ,CAAC/B,IAAI;QAC1BsB,CAAC,CAAC,YAAY;UACZA,CAAC,CAAC,UAAU,EACTU,SAAS,CAAC;YACTC,UAAU,EAAE,KAAK;YACjBC,YAAY,