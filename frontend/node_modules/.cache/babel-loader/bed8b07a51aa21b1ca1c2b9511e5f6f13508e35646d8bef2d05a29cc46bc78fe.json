         <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Data Nilai Ketrampilan</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <div class=\"card\">\r\n              <div class=\"card-header\">\r\n                <h3 class=\"card-title\"></h3>\r\n              </div>\r\n              <form>\r\n                <div class=\"card-body\">\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"nip\"\r\n                      >NIP:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users && users.wala\">\r\n                        {{ users.wala.guru.nip }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"nama\"\r\n                      >Nama:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users && users.wala\">\r\n                        {{ users.wala.guru.nama }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"kelas\"\r\n                      >Kelas:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <p v-if=\"users && users.wala\">\r\n                        {{ users.wala.kelas.name_kelas }}\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"form-group row\">\r\n                    <label class=\"col-sm-3 col-form-label\" for=\"mapel\"\r\n                      >Pilih Mata Pelajaran:</label\r\n                    >\r\n                    <div class=\"col-sm-9\">\r\n                      <select\r\n                        class=\"form-control select2\"\r\n                        v-if=\"filteredGuru\"\r\n                        v-model=\"selectedMapel\"\r\n                      >\r\n                        <option\r\n                          v-for=\"tes in filteredGuru\"\r\n                          :key=\"tes.id\"\r\n                          :value=\"tes\"\r\n                        >\r\n                          {{ tes.mapel.name_mapel }} - {{ tes.nama }}\r\n                        </option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n              <div v-if=\"selectedMapel && selectedMapel.kelas\">\r\n                <div v-for=\"kelas in selectedMapel.kelas\" :key=\"kelas.id\">\r\n                  <div class=\"card-header\">\r\n                    <h3 class=\"card-title\">Kelas: {{ kelas.name_kelas }}</h3>\r\n                  </div>\r\n                  <button @click=\"cetakPDF\">Cetak PDF</button>\r\n                  <div class=\"card-body\">\r\n                    <div class=\"table-responsive\">\r\n                      <table\r\n                        id=\"myTable\"\r\n                        class=\"table table-bordered table-hover\"\r\n                      >\r\n                        <thead>\r\n                          <tr>\r\n                            <th colSpan=\"32\" class=\"text-center\">\r\n                              Nilai Ketrampilan\r\n                            </th>\r\n                          </tr>\r\n                          <tr>\r\n                            <th rowSpan=\"2\" class=\"align-middle\">No</th>\r\n                            <th rowSpan=\"2\" class=\"align-middle text-center\">\r\n                              NIS\r\n                            </th>\r\n                            <th rowSpan=\"2\" class=\"align-middle text-center\">\r\n                              Siswa\r\n                            </th>\r\n                            <th colSpan=\"8\" class=\"text-center\">Proses</th>\r\n                            <th colSpan=\"8\" class=\"text-center\">Proyek</th>\r\n                            <th colSpan=\"8\" class=\"text-center\">Produk</th>\r\n                          </tr>\r\n                          <tr>\r\n                            <th class=\"text-center\">1</th>\r\n                            <th class=\"text-center\">2</th>\r\n                            <th class=\"text-center\">3</th>\r\n                            <th class=\"text-center\">4</th>\r\n                            <th class=\"text-center\">5</th>\r\n                            <th class=\"text-center\">6</th>\r\n                            <th class=\"text-center\">7</th>\r\n                            <th class=\"text-center\">8</th>\r\n                            <th class=\"text-center\">1</th>\r\n                            <th class=\"text-center\">2</th>\r\n                            <th class=\"text-center\">3</th>\r\n                            <th class=\"text-center\">4</th>\r\n                            <th class=\"text-center\">5</th>\r\n                            <th class=\"text-center\">6</th>\r\n                            <th class=\"text-center\">7</th>\r\n                            <th class=\"text-center\">8</th>\r\n                            <th class=\"text-center\">1</th>\r\n                            <th class=\"text-center\">2</th>\r\n                            <th class=\"text-center\">3</th>\r\n                            <th class=\"text-center\">4</th>\r\n                            <th class=\"text-center\">5</th>\r\n                            <th class=\"text-center\">6</th>\r\n                            <th class=\"text-center\">7</th>\r\n                            <th class=\"text-center\">8</th>\r\n                          </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                          <tr\r\n                            v-for=\"(child, index) in kelas.siswa\"\r\n                            :key=\"child.id\"\r\n                          >\r\n                            <td class=\"align-middle text-center\">\r\n                              {{ index + 1 }}\r\n                            </td>\r\n                            <td class=\"align-middle text-center\">\r\n                              {{ child.nis }}\r\n                            </td>\r\n                            <td class=\"align-middle text-center\">\r\n                              {{ child.name }}\r\n                            </td>\r\n                            <td\r\n                              v-for=\"i in 8\"\r\n                              :key=\"i\"\r\n                              class=\"align-middle text-center\"\r\n                            >\r\n                              {{\r\n                                getNilaiNK(\r\n                                  child.id,\r\n                                  selectedMapel.id,\r\n                                  \"proses\",\r\n                                  i\r\n                                )\r\n                              }}\r\n                            </td>\r\n                            <td\r\n                              v-for=\"i in 8\"\r\n                              :key=\"i\"\r\n                              class=\"align-middle text-center\"\r\n                            >\r\n                              {{\r\n                                getNilaiNK(\r\n                                  child.id,\r\n                                  selectedMapel.id,\r\n                                  \"proyek\",\r\n                                  i\r\n                                )\r\n                              }}\r\n                            </td>\r\n                            <td\r\n                              v-for=\"i in 8\"\r\n                              :key=\"i\"\r\n                              class=\"align-middle text-center\"\r\n                            >\r\n                              {{\r\n                                getNilaiNK(\r\n                                  child.id,\r\n                                  selectedMapel.id,\r\n                                  \"produk\",\r\n                                  i\r\n                                )\r\n                              }}\r\n                            </td>\r\n                          </tr>\r\n                        </tbody>\r\n                      </table>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport UserService from \"../../services/user.service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      content: null,\r\n      users: {},\r\n      selectedMapel: null,\r\n      filteredGuru: [],\r\n      filterguru: [],\r\n      nkData: [],\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n  mounted() {\r\n    UserService.getProfileWalas().then(\r\n      (response) => {\r\n        this.content = response.data;\r\n      },\r\n      (error) => {\r\n        this.content =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n  methods: {\r\n    getData() {\r\n      UserService.getProfileWalas()\r\n        .then((response) => {\r\n          this.users = response.data.user;\r\n          this.nkData = response.data.nk;\r\n          this.filterguru = response.data.filterdataguru;\r\n          const kelasId = this.users.wala.kelas.id;\r\n          this.filteredGuru = this.filterguru.filter((guru) => {\r\n            return guru.kelas[0].id === kelasId;\r\n          });\r\n\r\n          console.log(\"Users Data:\", this.users);\r\n          console.log(\"Filter Guru Data:\", this.filterguru);\r\n          console.log(\"Filtered Guru Data:\", this.filteredGuru);\r\n          console.log(\"NK Data:\", this.nkData);\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    // cetakPDF() {\r\n    //   const requestData = {\r\n    //     selectedMapel: this.selectedMapel,\r\n    //   };\r\n\r\n    //   UserService.CetakPDF(requestData, {\r\n    //     withCredentials: true,\r\n    //     responseType: \"blob\",\r\n    //   })\r\n    //     .then((response) => {\r\n    //       const namaMapel = this.selectedMapel.mapel.name_mapel;\r\n    //       const filename = `${namaMapel}_nilai_ketrampilan.pdf`;\r\n\r\n    //       const blob = new Blob([response.data], {\r\n    //         type: \"application/pdf\",\r\n    //       });\r\n    //       const url = window.URL.createObjectURL(blob);\r\n    //       const link = document.createElement(\"a\");\r\n    //       link.href = url;\r\n    //       link.setAttribute(\"download\", filename);\r\n    //       document.body.appendChild(link);\r\n    //       link.click();\r\n    //       window.URL.revokeObjectURL(url);\r\n    //     })\r\n    //     .catch((error) => {\r\n    //       console.error(\"Gagal mencetak PDF:\", error);\r\n    //     });\r\n    // },\r\n\r\n    cetakPDF() {\r\n      // Mendapatkan token dari penyimpanan lokal (localStorage)\r\n      const token = localStorage.getItem(\"user\");\r\n\r\n      if (!token) {\r\n        console.error(\"Token tidak ditemukan\");\r\n        alert(\"Token tidak ditemukan. Silakan login terlebih dahulu.\");\r\n        return;\r\n      }\r\n      // Ganti URL dengan URL endpoint Anda\r\n      const url = \"http://localhost:8080/api/pdfnilaiketrampilan\";\r\n\r\n      // Data yang akan dikirim dalam permintaan (jika diperlukan)\r\n      const requestData = {\r\n        // Data yang diperlukan oleh server, sesuai dengan permintaan Anda\r\n        // Misalnya: selectedMapel, token, atau data lain yang diperlukan.\r\n        selectedMapel: this.selectedMapel,\r\n      };\r\n\r\n      // Konfigurasi permintaan Axios\r\n      const config = {\r\n        responseType: \"blob\", // Atur responseType ke \"blob\" untuk menerima respons dalam bentuk blob\r\n        withCredentials: true, // Jika ada cookie yang perlu disertakan\r\n        headers: {\r\n          \"x-access-token\": token, // Menambahkan token ke header permintaan\r\n        },\r\n      };\r\n\r\n      // Melakukan permintaan ke server\r\n      axios\r\n        .post(url, requestData, config)\r\n        .then((response) => {\r\n          // Membuat blob PDF dari respons server\r\n          const blob = new Blob([response.data], {\r\n            type: \"application/pdf\",\r\n          });\r\n\r\n          // Menggunakan nama file yang diperoleh dari header respons\r\n          const contentDisposition = response.headers[\"content-disposition\"];\r\n          const filenameMatch = /filename[^;=\\n]*=((['\"]).*?\\2|[^;\\n]*)/.exec(\r\n            contentDisposition\r\n          );\r\n          const filename =\r\n            filenameMatch && filenameMatch[1]\r\n              ? filenameMatch[1]\r\n              : \"nilai_ketrampilan.pdf\";\r\n\r\n          // Membuat URL objek untuk blob PDF\r\n          const url = window.URL.createObjectURL(blob);\r\n\r\n          // Membuat elemen anchor untuk mendownload PDF\r\n          const link = document.createElement(\"a\");\r\n          link.href = url;\r\n          link.setAttribute(\"download\", filename);\r\n          document.body.appendChild(link);\r\n\r\n          // Mengklik elemen anchor untuk memulai pengunduhan\r\n          link.click();\r\n\r\n          // Merevoke URL objek setelah selesai\r\n          window.URL.revokeObjectURL(url);\r\n\r\n          // Sukses\r\n          alert(\"PDF berhasil dicetak\");\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Gagal mencetak PDF:\", error);\r\n          alert(\"Gagal mencetak PDF. Silakan coba lagi nanti.\");\r\n        });\r\n    },\r\n\r\n    // async cetakPDF() {\r\n    //   try {\r\n    //     const requestData = {\r\n    //       selectedMapel: this.selectedMapel,\r\n    //     };\r\n\r\n    //     // Ganti URL dengan URL endpoint Anda untuk mencetak PDF\r\n    //     const response = await axios.post(\r\n    //       \"http://localhost:8080/api/pdfnilaiketrampilan\",\r\n    //       requestData,\r\n    //       {\r\n    //         responseType: \"blob\", // Atur responseType ke \"blob\" untuk menerima respons dalam bentuk blob\r\n    //         withCredentials: true, // Jika ada cookie yang perlu disertakan\r\n    //       }\r\n    //     );\r\n\r\n    //     // Membuat blob PDF\r\n    //     const blob = new Blob([response.data], {\r\n    //       type: \"application/pdf\",\r\n    //     });\r\n\r\n    //     // Mengambil nama mapel dari selectedMapel\r\n    //     const namaMapel = this.selectedMapel.mapel.name_mapel;\r\n\r\n    //     // Membuat nama file PDF sesuai dengan nama mapel\r\n    //     const filename = `${namaMapel}_nilai_ketrampilan.pdf`;\r\n\r\n    //     // Membuat URL objek untuk blob PDF\r\n    //     const url = window.URL.createObjectURL(blob);\r\n\r\n    //     // Membuat elemen anchor untuk mendownload PDF\r\n    //     const link = document.createElement(\"a\");\r\n    //     link.href = url;\r\n    //     link.setAttribute(\"download\", filename); // Menggunakan nama file yang sudah dibuat\r\n    //     document.body.appendChild(link);\r\n\r\n    //     // Mengklik elemen anchor untuk memulai pengunduhan\r\n    //     link.click();\r\n\r\n    //     // Merevoke URL objek setelah selesai\r\n    //     window.URL.revokeObjectURL(url);\r\n\r\n    //     alert(\"PDF berhasil dicetak\");\r\n    //   } catch (error) {\r\n    //     console.error(\"Gagal mencetak PDF:\", error);\r\n    //     alert(\"Gagal mencetak PDF. Silakan coba lagi nanti.\");\r\n    //   }\r\n    // },\r\n\r\n    getNilaiNK(siswaId, guruId, jenis, i) {\r\n      const nilai = this.nkData.find(\r\n        (item) => item.siswaId === siswaId && item.guruId === guruId\r\n      );\r\n      if (nilai) {\r\n        return nilai[jenis + i] || \"-\";\r\n      }\r\n      return \"-\";\r\n    },\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}