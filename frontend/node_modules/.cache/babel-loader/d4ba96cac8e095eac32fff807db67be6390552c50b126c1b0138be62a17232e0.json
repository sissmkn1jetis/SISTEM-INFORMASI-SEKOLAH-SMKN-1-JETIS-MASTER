 <button\r\n                  type=\"button\"\r\n                  class=\"btn btn-success mb-2\"\r\n                  @click=\"showModal\"\r\n                >\r\n                  Tambah User\r\n                </button>\r\n                <table id=\"example2\" class=\"table table-bordered table-hover\">\r\n                  <thead>\r\n                    <tr>\r\n                      <th>No</th>\r\n                      <th>Username</th>\r\n                      <th>Email</th>\r\n                      <th>Role</th>\r\n                      <th>Action</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"(user, index) in users\" :key=\"index.id\">\r\n                      <td>{{ ++index }}</td>\r\n                      <td>{{ user.username }}</td>\r\n                      <td>{{ user.email }}</td>\r\n                      <td v-for=\"role in user.roles\" :key=\"role\">\r\n                        {{ role.name }}\r\n                      </td>\r\n                      <td>Edit | Hapus</td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"modal fade\" id=\"modalmuncul\">\r\n        <div class=\"modal-dialog modal-lg\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h4 class=\"modal-title\">Tambah User</h4>\r\n              <button\r\n                type=\"button\"\r\n                class=\"close\"\r\n                data-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <span aria-hidden=\"true\">&times;</span>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"card card-default\">\r\n                <!-- /.card-header -->\r\n                <!-- form start -->\r\n                <form>\r\n                  <div class=\"card-body\">\r\n                    <div class=\"form-group\">\r\n                      <label for=\"exampleInputUsername1\">Username</label>\r\n                      <input\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputUsername1\"\r\n                        placeholder=\"Username\"\r\n                        v-model=\"form.username\"\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"exampleInputEmail1\">Email address</label>\r\n                      <input\r\n                        type=\"email\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputEmail1\"\r\n                        placeholder=\"Enter email\"\r\n                        v-model=\"form.email\"\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"exampleInputPassword1\">Password</label>\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputPassword1\"\r\n                        placeholder=\"Password\"\r\n                        v-model=\"form.password\"\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"exampleInputPassword1\"\r\n                        >Confirmasi Password</label\r\n                      >\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputPassword1\"\r\n                        placeholder=\"Confirmasi Password\"\r\n                        v-model=\"form.confpassword\"\r\n                      />\r\n                    </div>\r\n\r\n                    <div class=\"form-group\">\r\n                      <label>Role</label>\r\n                      <select class=\"form-control\" v-model=\"form.roles\">\r\n                        <option\r\n                          v-for=\"role in roles\"\r\n                          :key=\"role.id\"\r\n                          :value=\"role.id\"\r\n                        >\r\n                          {{ role.name }}\r\n                        </option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n            <div class=\"modal-footer\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-secondary\"\r\n                data-dismiss=\"modal\"\r\n              >\r\n                Close\r\n              </button>\r\n              <button class=\"btn btn-primary\">Simpan</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport User from \"../../services/user.service\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      users: {},\r\n      roles: {},\r\n      form: {\r\n        id: \"\",\r\n        username: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n        confpassword: \"\",\r\n        roles: \"\",\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.getdata();\r\n  },\r\n  methods: {\r\n    showModal() {\r\n      this.form.reset();\r\n      $(\"#modalmuncul\").modal(\"show\");\r\n    },\r\n    getdata() {\r\n      User.getUser()\r\n        .then((response) => {\r\n          this.users = response.data;\r\n        })\r\n        .catch((e) => {\r\n          this.users =\r\n            (e.response && e.response.data && e.response.data.message) ||\r\n            e.message ||\r\n            e.toString();\r\n        });\r\n      User.Role()\r\n        .then((response) => {\r\n          this.roles = response.data;\r\n        })\r\n        .catch((e) => {\r\n          this.roles =\r\n            (e.response && e.response.data && e.response.data.message) ||\r\n            e.message ||\r\n            e.toString();\r\n        });\r\n    },\r\n    simpanData() {\r\n      this.form;\r\n      User.create(data)\r\n        .then((response) => {\r\n          this.form.id = response.data.id;\r\n          $(\"#modalmuncul\").modal(\"show\");\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}