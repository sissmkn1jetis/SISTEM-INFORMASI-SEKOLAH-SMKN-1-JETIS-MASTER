n            name:\"\",\r\n            tgl_lahir:\"\",\r\n            jkg:\"\",\r\n            alamat:\"\"\r\n        },\r\n        message: ''\r\n      };\r\n    },\r\n    created: function () {\r\n      this.message = '';\r\n      this.getGuruById(this.$route.params.id);\r\n    },\r\n    methods: {\r\n        getGuruById(id) {\r\n        GuruService.get(id)\r\n            .then(response => {\r\n            this.dataguru = response.data;\r\n            })\r\n            .catch(e => {\r\n            console.log(e);\r\n            });\r\n        },\r\n        updateGuru() {\r\n            GuruService.update(this.dataguru.id, this.dataguru)\r\n                .then(response => {\r\n                console.log(response.data);\r\n                this.message = 'The Guru was updated successfully!';\r\n                this.$router.push(\"/gurulist\");\r\n                })\r\n                .catch(e => {\r\n                console.log(e);\r\n                });\r\n            },\r\n    },\r\n  };\r\n  </script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}