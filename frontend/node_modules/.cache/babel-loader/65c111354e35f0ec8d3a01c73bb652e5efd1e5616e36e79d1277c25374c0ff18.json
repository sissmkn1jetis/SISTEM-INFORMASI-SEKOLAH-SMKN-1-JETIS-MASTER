{"ast":null,"code":"import UserService from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      content: null,\n      users: [],\n      selectedMapel: null,\n      filterguru: [],\n      nkData: []\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getData();\n    this.selectMapel();\n  },\n  mounted() {\n    UserService.getProfileWalas().then(response => {\n      this.content = response.data;\n    }, error => {\n      this.content = error.response && error.response.data && error.response.data.message || error.message || error.toString();\n    });\n  },\n  methods: {\n    getData() {\n      UserService.getProfileWalas().then(response => {\n        this.users = response.data.user;\n        this.filterguru = response.data.filterdataguru;\n        this.nkData = response.data.nk;\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n      UserService.getProfileWalas().then(response => {\n        this.content = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    getNilaiNK(siswaId, jenis, i) {\n      const nilai = this.nkData.find(item => item.siswaId === siswaId);\n      if (nilai) {\n        return nilai[jenis + i] || \"-\";\n      }\n      return \"-\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAyLA,OAAOA,WAAU,MAAO,6BAA6B;AACrD,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE,IAAI;MACnBC,UAAU,EAAE,EAAE;MACdC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;IACd,IAAI,CAACC,WAAW,EAAE;EACpB,CAAC;EACDC,OAAO,GAAG;IACRhB,WAAW,CAACiB,eAAe,EAAE,CAACC,IAAI,CAC/BC,QAAQ,IAAK;MACZ,IAAI,CAACjB,OAAM,GAAIiB,QAAQ,CAAClB,IAAI;IAC9B,CAAC,EACAmB,KAAK,IAAK;MACT,IAAI,CAAClB,OAAM,GACRkB,KAAK,CAACD,QAAO,IACZC,KAAK,CAACD,QAAQ,CAACl