{"ast":null,"code":"import User from \"../../services/user.service\";\nexport default {\n  data() {\n    return {\n      users: {},\n      form: {\n        id: \"\",\n        username: \"\",\n        email: \"\",\n        password: \"\",\n        confPassword: \"\",\n        role: \"\"\n      },\n      successful: false,\n      loading: false,\n      message: \"\"\n      // submitted: false,\n    };\n  },\n\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getdata();\n  },\n  methods: {\n    showModal() {\n      // this.form.reset();\n      $(\"#modalmuncul\").modal(\"show\");\n    },\n    getdata() {\n      User.getUser().then(response => {\n        this.users = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    simpanData() {\n      var form = {\n        username: this.form.username,\n        email: this.form.email,\n        password: this.form.password,\n        confPassword: this.form.confPassword,\n        role: this.form.role\n      };\n      this.message = \"\";\n      this.successful = false;\n      this.loading = true;\n      User.create(form).then(response => {\n        this.form.id = response.form.id;\n        console.log(response.form);\n        // this.submitted = true;\n        $(\"#modalmuncul\").modal(\"hide\");\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAqKA,OAAOA,IAAG,MAAO,6BAA6B;AAC9C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC;MACTC,IAAI,EAAE;QACJC,EAAE,EAAE,EAAE;QACNC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,IAAI,EAAE;MACR,CAAC;MACDC,UAAU,EAAE,KAAK;MACjBC,OAAO,EAAE,KAAK;MACdC,OAAO,EAAE;MACT;IACF,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV;MACAC,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;IACjC,CAAC;IACDJ