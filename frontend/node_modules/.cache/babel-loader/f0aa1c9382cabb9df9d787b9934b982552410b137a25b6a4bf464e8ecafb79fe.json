{"ast":null,"code":"export default {\n  name: \"Clock\",\n  data() {\n    return {\n      hourRotation: 0,\n      minuteRotation: 0,\n      secondRotation: 0,\n      isLightMode: false\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    },\n    clockClass() {\n      return this.isLightMode ? \"light\" : \"\";\n    }\n  },\n  mounted() {\n    this.startClock();\n  },\n  beforeDestroy() {\n    clearInterval(this.clockInterval);\n  },\n  methods: {\n    toggleClass() {\n      this.isLightMode = !this.isLightMode;\n    },\n    startClock() {\n      this.updateClock();\n      this.clockInterval = setInterval(this.updateClock, 1000);\n    },\n    updateClock() {\n      const now = new Date();\n      const hh = now.getHours() % 12 * 30;\n      const mm = now.getMinutes() * 6;\n      const ss = now.getSeconds() * 6;\n      this.hourRotation = hh + mm / 12;\n      this.minuteRotation = mm;\n      this.secondRotation = ss;\n    }\n  }\n};","map":{"version":3,"mappings":"AAkIA,eAAe;EACbA,IAAI,EAAE,OAAO;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;MACfC,cAAc,EAAE,CAAC;MACjBC,cAAc,EAAE,CAAC;MACjBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC,CAAC;IACDC,UAAU,GAAG;MACX,OAAO,IAAI,CAACP,WAAU,GAAI,OAAM,GAAI,EAAE;IACxC;EACF,CAAC;EACDQ,OAAO,GAAG;IACR,IAAI,CAACC,UAAU,EAAE;EACnB,CAAC;EACDC,aAAa,GAAG;IACdC,aAAa,CAAC,IAAI,CAACC,aAAa,CAAC;EACnC,CAAC;EACDC,OAAO,EAAE;IACPC,WAAW,GAAG;MACZ,IAAI,CAACd,WAAU,GAAI,CAAC,IAAI,CAACA,WAAW;IACtC,CAAC;IACDS,UAAU,GAAG;MACX,IAAI,CAACM,WAAW,EAAE;MAClB,IAAI,CAACH,aAAY,GAAII,WAAW,CAAC,IAAI,CAACD,WAAW,EAAE,IAAI,CAAC;IAC1D,CAAC;IACDA,WAAW,GAAG;MACZ,MAAME,GAAE,GAAI,IAAIC,IAAI,EAAE;MACtB,MAAMC,EAAC,GAAKF,GAAG,CAACG,QAAQ,EAAC,GAAI,EAAE,GAAI,EAAE;MACrC,MAAMC,EAAC,GAAIJ,GAAG,CAACK,UAAU,EAAC,GAAI,CAAC;MAC/B,MAAMC,EAAC,GAAIN,GAAG,CAACO,UAAU,EAAC,GAAI,CAAC;MAE/B,IAAI,CAAC3B,YAAW,GAAIsB,EAAC,GAAIE,EAAC,GAAI,EAAE;MAChC,IAAI,CAACvB,cAAa,GAAIuB,EAAE;MACxB,IAAI,CAACtB,cAAa,GAAIwB,EAAE;IAC1B;EACF;AACF,CAAC","names":["name","data","hourRotation","minuteRotation","secondRotation","isLightMode","computed","currentUser","$store","state","auth","user","clockClass","mounted","startClock","beforeDestroy","clearInterval","clockInterval","methods","toggleClass","updateClock","setInterval","now","Date","hh","getHours","mm","getMinutes","ss","getSeconds"],"sourceRoot":"","sources":["D:\\filee\\SISTEM-INFORMASI-SEKOLAH-SMKN-1-JETIS-MASTER\\frontend\\src\\components\\Dashboard.vue"],"sourcesContent":["<template>\r\n  <!-- <div class=\"container\">\r\n    <header class=\"jumbotron\">\r\n      <h3>\r\n        <strong>{{ currentUser.username }}</strong> Profile\r\n      </h3>\r\n    </header>\r\n    <p>\r\n      <strong>Token:</strong>\r\n      {{ currentUser.accessToken.substring(0, 20) }} ...\r\n      {{ currentUser.accessToken.substr(currentUser.accessToken.length - 20) }}\r\n    </p>\r\n    <p>\r\n      <strong>Id:</strong>\r\n      {{ currentUser.id }}\r\n    </p>\r\n    <p>\r\n      <strong>Email:</strong>\r\n      {{ currentUser.email }}\r\n    </p>\r\n    <strong>Authorities:</strong>\r\n    <ul>\r\n      <li v-for=\"role in currentUser.roles\" :key=\"role\">{{ role }}</li>\r\n    </ul>\r\n  </div> -->\r\n  <div>\r\n    <!-- Content Header (Page header) -->\r\n    <section class=\"content-header\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row mb-2\">\r\n          <div class=\"col-sm-6\">\r\n            <h1>Dashboard</h1>\r\n          </div>\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Dashboard</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Main content -->\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-8\">\r\n            <div v-if=\"currentUser.role === 'admin'\">\r\n              <!-- Konten untuk Admin -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Admin</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  <p class=\"welcome-msg\">\r\n                    Selamat Datang <strong>{{ currentUser.username }}</strong\r\n                    >, Anda Login Sebagai\r\n                    <strong>{{ currentUser.role }}</strong>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else-if=\"currentUser.role === 'guru'\">\r\n              <!-- Konten untuk Guru -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Guru</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  Selamat Datang\r\n                  <strong>{{ currentUser.username }}</strong> Anda Login Sebagai\r\n                  <strong>{{ currentUser.role }}</strong>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else-if=\"currentUser.role === 'walas'\">\r\n              <!-- Konten untuk Wali Kelas -->\r\n              <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                  <h3 class=\"card-title\">Wali Kelas</h3>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                  Selamat Datang\r\n                  <strong>{{ currentUser.username }}</strong> Anda Login Sebagai\r\n                  <strong>{{ currentUser.role }}</strong>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-else>\r\n              <p>Anda tidak memiliki akses ke dashboard ini.</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-md-4\">\r\n            <!-- Tampilan jam dan tanggal di samping card utama -->\r\n            <div class=\"card\">\r\n              <div class=\"card-body text-center\">\r\n                <div class=\"clock\">\r\n                  <div class=\"hour\">\r\n                    <div\r\n                      class=\"hr\"\r\n                      :style=\"{ transform: `rotateZ(${hourRotation}deg)` }\"\r\n                    ></div>\r\n                  </div>\r\n                  <div class=\"min\">\r\n                    <div\r\n                      class=\"mn\"\r\n                      :style=\"{ transform: `rotateZ(${minuteRotation}deg)` }\"\r\n                    ></div>\r\n                  </div>\r\n                  <div class=\"sec\">\r\n                    <div\r\n                      class=\"sc\"\r\n                      :style=\"{ transform: `rotateZ(${secondRotation}deg)` }\"\r\n                    ></div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"toggleClass\" @click=\"toggleClass\"></div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Clock\",\r\n  data() {\r\n    return {\r\n      hourRotation: 0,\r\n      minuteRotation: 0,\r\n      secondRotation: 0,\r\n      isLightMode: false,\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n    clockClass() {\r\n      return this.isLightMode ? \"light\" : \"\";\r\n    },\r\n  },\r\n  mounted() {\r\n    this.startClock();\r\n  },\r\n  beforeDestroy() {\r\n    clearInterval(this.clockInterval);\r\n  },\r\n  methods: {\r\n    toggleClass() {\r\n      this.isLightMode = !this.isLightMode;\r\n    },\r\n    startClock() {\r\n      this.updateClock();\r\n      this.clockInterval = setInterval(this.updateClock, 1000);\r\n    },\r\n    updateClock() {\r\n      const now = new Date();\r\n      const hh = (now.getHours() % 12) * 30;\r\n      const mm = now.getMinutes() * 6;\r\n      const ss = now.getSeconds() * 6;\r\n\r\n      this.hourRotation = hh + mm / 12;\r\n      this.minuteRotation = mm;\r\n      this.secondRotation = ss;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Gaya tambahan untuk membuat tampilan jam analog yang mirip */\r\n.clock {\r\n  width: 350px;\r\n  height: 350px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background: #091921 url(~@/assets/clock.png);\r\n  background-size: cover;\r\n  border: 4px solid #091921;\r\n  border-radius: 50%;\r\n  box-shadow: 0 -15px 15px rgba(255, 255, 255, 0.05),\r\n    inset 0 -15px 15px rgba(255, 255, 255, 0.05),\r\n    0 15px 15px rgba(0, 0, 0, 0.05), inset 0 15px 15px rgba(0, 0, 0, 0.05);\r\n}\r\n\r\n.light .clock {\r\n  background: #d1dae3 url(~@/assets/clock.png);\r\n  background-size: cover;\r\n  border: 4px solid #cad3dc;\r\n  box-shadow: -8px -8px 15px rgba(255, 255, 255, 0.5),\r\n    10px 10px 10px rgba(0, 0, 0, 0.1),\r\n    inset -8px -8px 15px rgba(255, 255, 255, 0.5),\r\n    inset 10px 10px 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.clock::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  width: 15px;\r\n  height: 15px;\r\n  background: #fff;\r\n  z-index: 100000;\r\n  border-radius: 50%;\r\n}\r\n\r\n.light .clock::before {\r\n  background: #008eff;\r\n}\r\n\r\n.hour,\r\n.min,\r\n.sec {\r\n  position: absolute;\r\n}\r\n\r\n.hour,\r\n.hr {\r\n  width: 160px;\r\n  height: 160px;\r\n}\r\n\r\n.min,\r\n.mn {\r\n  width: 190px;\r\n  height: 190px;\r\n}\r\n\r\n.sec,\r\n.sc {\r\n  width: 230px;\r\n  height: 230px;\r\n}\r\n\r\n.hr,\r\n.mn,\r\n.sc {\r\n  display: flex;\r\n  justify-content: center;\r\n  position: absolute;\r\n  border-radius: 50%;\r\n}\r\n\r\n.hr::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  width: 8px;\r\n  height: 80px;\r\n  background: #848484;\r\n  z-index: 10;\r\n  border-radius: 6px 6px 0 0;\r\n}\r\n\r\n.mn::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  width: 4px;\r\n  height: 90px;\r\n  background: #d6d6d6;\r\n  z-index: 11;\r\n  border-radius: 6px 6px 0 0;\r\n}\r\n\r\n.light .mn::before {\r\n  background: #091921;\r\n}\r\n\r\n.sc::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  width: 2px;\r\n  height: 150px;\r\n  background: #008eff;\r\n  z-index: 12;\r\n  border-radius: 6px 6px 0 0;\r\n}\r\n\r\n.toggleClass {\r\n  position: absolute;\r\n  top: 10%;\r\n  right: 150px;\r\n  width: 20px;\r\n  height: 20px;\r\n  font-size: 18px;\r\n  border-radius: 50%;\r\n  background: #d1dae3;\r\n  color: #d1dae3;\r\n  font-family: consolas;\r\n  cursor: pointer;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.toggleClass::before {\r\n  position: absolute;\r\n  left: 25px;\r\n  content: \"Light Mode\";\r\n  white-space: nowrap;\r\n}\r\n\r\n.light .toggleClass {\r\n  background: #091921;\r\n  color: #091921;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}