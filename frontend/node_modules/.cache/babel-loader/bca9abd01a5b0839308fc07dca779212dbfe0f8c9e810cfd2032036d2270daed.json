ppend\">\r\n          <button\r\n            class=\"btn btn-outline-secondary\"\r\n            type=\"button\"\r\n            @click=\"\r\n              page = 1;\r\n              retrieveTutorials();\r\n            \"\r\n          >\r\n            Search\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"col-md-12\">\r\n      <div class=\"mb-3\">\r\n        Items per Page:\r\n        <select v-model=\"pageSize\" @change=\"handlePageSizeChange($event)\">\r\n          <option v-for=\"size in pageSizes\" :key=\"size\" :value=\"size\">\r\n            {{ size }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n\r\n      <b-pagination\r\n        v-model=\"page\"\r\n        :total-rows=\"count\"\r\n        :per-page=\"pageSize\"\r\n        prev-text=\"Prev\"\r\n        next-text=\"Next\"\r\n        @change=\"handlePageChange\"\r\n      ></b-pagination>\r\n    </div>\r\n\r\n    <div class=\"col-md-6\">\r\n      <table id=\"example2\" class=\"table table-bordered table-hover\">\r\n        <thead>\r\n          <tr>\r\n            <th class=\"align-middle text-center\">No</th>\r\n            <th class=\"align-middle text-center\">Mata Pelajaran</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(mapel, index) in mapels\" :key=\"index.id\">\r\n            <td class=\"align-middle text-center\">{{ index + 1 }}</td>\r\n            <td class=\"align-middle text-center\">\r\n              {{ mapel.name_mapel }}\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n\r\n    <!-- <div class=\"col-md-6\">\r\n      <div v-if=\"currentTutorial\">\r\n        <h4>Tutorial</h4>\r\n        <div>\r\n          <label><strong>Title:</strong></label> {{ currentTutorial.title }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Description:</strong></label>\r\n          {{ currentTutorial.description }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Status:</strong></label>\r\n          {{ currentTutorial.published ? \"Published\" : \"Pending\" }}\r\n        </div>\r\n\r\n        <a\r\n          class=\"badge badge-warning\"\r\n          :href=\"'/tutorials/' + currentTutorial.id\"\r\n        >\r\n          Edit\r\n        </a>\r\n      </div>\r\n      <div v-else>\r\n        <br />\r\n        <p>Please click on a Tutorial...</p>\r\n      </div>\r\n    </div> -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MapelService from \"../../services/mapel.service\";\r\nexport default {\r\n  name: \"mapels-list\",\r\n  data() {\r\n    return {\r\n      mapels: [],\r\n      currentTutorial: null,\r\n      currentIndex: -1,\r\n      searchTitle: \"\",\r\n      page: 1,\r\n      count: 0,\r\n      pageSize: 3,\r\n      pageSizes: [3, 6, 9],\r\n    };\r\n  },\r\n  methods: {\r\n    getRequestParams(searchTitle, page, pageSize) {\r\n      let params = {};\r\n      if (searchTitle) {\r\n        params[\"title\"] = searchTitle;\r\n      }\r\n      if (page) {\r\n        params[\"page\"] = page - 1;\r\n      }\r\n      if (pageSize) {\r\n        params[\"size\"] = pageSize;\r\n      }\r\n      return params;\r\n    },\r\n    retrieveTutorials() {\r\n      const params = this.getRequestParams(\r\n        this.searchTitle,\r\n        this.page,\r\n        this.pageSize\r\n      );\r\n      MapelService.getMapel(params)\r\n        .then((response) => {\r\n          const { tutorials, totalItems } = response.data;\r\n          //   this.mapels = response.data;\r\n          this.tutorials = tutorials;\r\n          this.count = totalItems;\r\n          console.log(response.data);\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    handlePageChange(value) {\r\n      this.page = value;\r\n      this.retrieveTutorials();\r\n    },\r\n    handlePageSizeChange(event) {\r\n      this.pageSize = event.target.value;\r\n      this.page = 1;\r\n      this.retrieveTutorials();\r\n    },\r\n    refreshList() {\r\n      this.retrieveTutorials();\r\n      this.currentTutorial = null;\r\n      this.currentIndex = -1;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.retrieveTutorials();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.list {\r\n  text-align: left;\r\n  max-width: 750px;\r\n  margin: auto;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}