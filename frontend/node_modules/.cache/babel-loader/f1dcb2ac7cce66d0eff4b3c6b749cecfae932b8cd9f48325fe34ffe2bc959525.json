  <div class=\"card-body\">\r\n              <p>\r\n                Selamat Datang <strong>{{ currentUser.username }}</strong\r\n                >, Anda Login Sebagai <strong>{{ currentUser.role }}</strong>\r\n              </p>\r\n              <div class=\"clock\">\r\n                <div class=\"time\">{{ currentTime }}</div>\r\n                <div class=\"date\">{{ currentDate }}</div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-else-if=\"currentUser.role === 'guru'\">\r\n          <!-- Konten untuk Guru -->\r\n          <div class=\"card\">\r\n            <div class=\"card-header\">\r\n              <h3 class=\"card-title\">Guru</h3>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              Selamat Datang <strong>{{ currentUser.username }}</strong> Anda\r\n              Login Sebagai <strong>{{ currentUser.role }}</strong>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-else-if=\"currentUser.role === 'walas'\">\r\n          <!-- Konten untuk Wali Kelas -->\r\n          <div class=\"card\">\r\n            <div class=\"card-header\">\r\n              <h3 class=\"card-title\">Wali Kelas</h3>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              Selamat Datang <strong>{{ currentUser.username }}</strong> Anda\r\n              Login Sebagai <strong>{{ currentUser.role }}</strong>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div v-else>\r\n          <p>Anda tidak memiliki akses ke dashboard ini.</p>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Dashboard\",\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n    currentTime() {\r\n      const now = new Date();\r\n      const hours = now.getHours().toString().padStart(2, \"0\");\r\n      const minutes = now.getMinutes().toString().padStart(2, \"0\");\r\n      const seconds = now.getSeconds().toString().padStart(2, \"0\");\r\n      return `${hours}:${minutes}:${seconds}`;\r\n    },\r\n    currentDate() {\r\n      const now = new Date();\r\n      const options = {\r\n        weekday: \"long\",\r\n        year: \"numeric\",\r\n        month: \"long\",\r\n        day: \"numeric\",\r\n      };\r\n      return now.toLocaleDateString(\"id-ID\", options);\r\n    },\r\n  },\r\n  mounted() {\r\n    if (!this.currentUser) {\r\n      this.$router.push(\"/login\");\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Gaya untuk jam digital dan tanggal */\r\n.clock {\r\n  font-size: 2em;\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.time {\r\n  font-weight: bold;\r\n}\r\n\r\n.date {\r\n  font-style: italic;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}