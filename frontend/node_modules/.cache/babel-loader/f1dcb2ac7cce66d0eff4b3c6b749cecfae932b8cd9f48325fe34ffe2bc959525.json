{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Dashboard\",\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    },\n    currentTime() {\n      const now = new Date();\n      const hours = now.getHours().toString().padStart(2, \"0\");\n      const minutes = now.getMinutes().toString().padStart(2, \"0\");\n      const seconds = now.getSeconds().toString().padStart(2, \"0\");\n      return `${hours}:${minutes}:${seconds}`;\n    },\n    currentDate() {\n      const now = new Date();\n      const options = {\n        weekday: \"long\",\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\"\n      };\n      return now.toLocaleDateString(\"id-ID\", options);\n    }\n  },\n  mounted() {\n    if (!this.currentUser) {\n      this.$router.push(\"/login\");\n    }\n  }\n};","map":{"version":3,"mappings":";AAmGA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC,CAAC;IACDC,WAAW,GAAG;MACZ,MAAMC,GAAE,GAAI,IAAIC,IAAI,EAAE;MACtB,MAAMC,KAAI,GAAIF,GAAG,CAACG,QAAQ,EAAE,CAACC,QAAQ,EAAE,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MACxD,MAAMC,OAAM,GAAIN,GAAG,CAACO,UAAU,EAAE,CAACH,QAAQ,EAAE,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAC5D,MAAMG,OAAM,GAAIR,GAAG,CAACS,UAAU,EAAE,CAACL,QAAQ,EAAE,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAC5D,OAAQ,GAAEH,KAAM,IAAGI,OAAQ,IAAGE,OAAQ,EAAC;IACzC,CAAC;IACDE,WAAW,GAAG;MACZ,MAAMV,GAAE,GAAI,IAAIC,IAAI,EAAE;MACtB,MAAMU,OAAM,GAAI;QACdC,OAAO,EAAE,MAAM;QACfC,IAAI,EAAE,SAAS;QACfC,KAAK,EAAE,MAAM;QACbC,GAAG,EAAE;MACP,CAAC;MACD,OAAOf,GAAG,CAACgB,kBAAkB,CAAC,OAAO,EAAEL,OAAO,CAAC;IACjD;EACF,CAAC;EACDM,OAAO,GAAG;IACR,IAAI,CAAC,IAAI,CAACvB,WAAW,EAAE;MACrB,IAAI,CAACwB,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B;EACF;AACF,CAAC","names":["name","computed","currentUser","$store","state","auth","user","currentTime","now","Date","hours","getHours","toString","padStart","minutes","getMinutes","seconds","getSeconds","currentD