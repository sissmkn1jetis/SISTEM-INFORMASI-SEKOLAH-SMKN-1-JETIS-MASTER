    <h1>Data Guru</h1>\r\n          </div>\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Data Guru</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <div class=\"card\">\r\n              <div class=\"card-header\">\r\n                <div class=\"card-tools\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-success mb-2\"\r\n                    @click=\"showModal\"\r\n                  >\r\n                    Tambah Guru\r\n                  </button>\r\n                </div>\r\n              </div>\r\n              <div class=\"card-body\">\r\n                <div class=\"table-responsive\">\r\n                  <table id=\"example2\" class=\"table table-bordered table-hover\">\r\n                    <thead>\r\n                      <tr>\r\n                        <th class=\"align-middle text-center\">No</th>\r\n                        <th class=\"align-middle text-center\">NIP</th>\r\n                        <th class=\"align-middle text-center\">Name</th>\r\n                        <th class=\"align-middle text-center\">Tanggal Lahir</th>\r\n                        <th class=\"align-middle text-center\">Jenis Kelamin</th>\r\n                        <th class=\"align-middle text-center\">Alamat</th>\r\n                        <th class=\"align-middle text-center\">Mengajar</th>\r\n                        <th class=\"align-middle text-center\">Username</th>\r\n                        <th class=\"align-middle text-center\">Email</th>\r\n                        <th class=\"align-middle text-center\">Role</th>\r\n                        <th class=\"align-middle text-center\">Action</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"(guru, index) in gurus\" :key=\"index.id\">\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ index + 1 }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">{{ guru.nip }}</td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ guru.name }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ guru.tgl_lahir }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">{{ guru.jkg }}</td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ guru.alamat }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ guru.mapel.name_mapel }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ guru.user.username }}\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          {{ guru.user.email }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"guru.user && guru.user.role === 'guru'\"\r\n                        >\r\n                          <span class=\"badge bg-warning\">{{\r\n                            guru.user.role\r\n                          }}</span>\r\n                        </td>\r\n                        <td class=\"align-middle text-center\">\r\n                          <a\r\n                            href=\"#\"\r\n                            class=\"badge bg-info mr-1\"\r\n                            @click=\"showModalEdit(guru.id)\"\r\n                            ><i class=\"fas fa-edit\"></i\r\n                          ></a>\r\n                          <a\r\n                            href=\"#\"\r\n                            class=\"badge bg-danger\"\r\n                            @click=\"deleteData(guru.id)\"\r\n                            ><i class=\"fas fa-trash-alt\"></i\r\n                          ></a>\r\n                        </td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"modalmuncul\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"modalmuncul1\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-lg\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"!statusmodal\"\r\n              >\r\n                Tambah Pengguna\r\n              </h5>\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"statusmodal\"\r\n              >\r\n                Ubah Pengguna\r\n              </h5>\r\n              <button\r\n                type=\"button\"\r\n                class=\"close\"\r\n                data-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <span aria-hidden=\"true\" @click=\"resetForm()\">&times;</span>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"card card-default\">\r\n                <!-- /.card-header -->\r\n                <!-- form start -->\r\n                <form\r\n                  @submit.prevent=\"statusmodal ? updateData() : simpanData()\"\r\n                >\r\n                  <div class=\"card-body\">\r\n                    <p v-if=\"message\" class=\"alert-danger\">\r\n                      {{ message }}\r\n                    </p>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"nip\">NIP</label>\r\n                      <input\r\n                        type=\"number\"\r\n                        class=\"form-control\"\r\n                        id=\"nip\"\r\n                        placeholder=\"NIP\"\r\n                        v-model=\"form.nip\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"name\">Name</label>\r\n                      <input\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        id=\"name\"\r\n                        placeholder=\"Name\"\r\n                        v-model=\"form.name\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"tgl_lahir\">Tanggal Lahir</label>\r\n                      <input\r\n                        type=\"date\"\r\n                        class=\"form-control\"\r\n                        id=\"tgl_lahir\"\r\n                        placeholder=\"Tanggal Lahir\"\r\n                        v-model=\"form.tgl_lahir\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label>Jenis Kelamin</label>\r\n                      <select\r\n     