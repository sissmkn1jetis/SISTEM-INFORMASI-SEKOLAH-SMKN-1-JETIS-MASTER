ceRoot":"","sources":["D:\\filee\\TA APP MODUL PENILAIAN SISWA\\frontend\\src\\components\\DataGuru\\Tesguru.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <section class=\"content-header\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row mb-2\">\r\n          <div class=\"col-sm-6\">\r\n            <h1>Data User</h1>\r\n          </div>\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/dashboard\">Dashboard</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\">Data User</li>\r\n            </ol>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <section class=\"content\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <div class=\"card\">\r\n              <div class=\"card-header\">\r\n                <div class=\"card-tools\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-success mb-2\"\r\n                    @click=\"showModal\"\r\n                  >\r\n                    Tambah User\r\n                  </button>\r\n                </div>\r\n              </div>\r\n              <div class=\"card-body\">\r\n                <div class=\"table-responsive\">\r\n                  <table id=\"example2\" class=\"table table-bordered table-hover\">\r\n                    <thead>\r\n                      <tr>\r\n                        <th class=\"align-middle text-center\">NIP</th>\r\n                        <th class=\"align-middle text-center\">Name</th>\r\n                        <th class=\"align-middle text-center\">Username</th>\r\n                        <th class=\"align-middle text-center\">Email</th>\r\n                        <th class=\"align-middle text-center\">Role</th>\r\n                        <th class=\"align-middle text-center\">Tanggal Lahir</th>\r\n                        <th class=\"align-middle text-center\">Jenis Kelamin</th>\r\n                        <th class=\"align-middle text-center\">Alamat</th>\r\n                        <th class=\"align-middle text-center\">Mengajar</th>\r\n                        <th class=\"align-middle text-center\">Action</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"user in users\" :key=\"user.id\">\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          {{ user.username }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          {{ user.name }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          {{ user.email }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          <span class=\"badge bg-warning\">{{ user.role }}</span>\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          {{ user.nip }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          {{ user.tgl_lahir }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          {{ user.jkg }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          {{ user.alamat }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          {{ user.mapel.name_mapel }}\r\n                        </td>\r\n                        <td\r\n                          class=\"align-middle text-center\"\r\n                          v-if=\"user && user.role === 'guru'\"\r\n                        >\r\n                          <a\r\n                            href=\"#\"\r\n                            class=\"badge bg-info mr-1\"\r\n                            @click=\"showModalEdit(user.id)\"\r\n                            ><i class=\"fas fa-edit\"></i\r\n                          ></a>\r\n                          <a\r\n                            href=\"#\"\r\n                            class=\"badge bg-danger\"\r\n                            @click=\"deleteData(user.id)\"\r\n                            ><i class=\"fas fa-trash-alt\"></i\r\n                          ></a>\r\n                        </td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"modalmuncul\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"modalmuncul1\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <div class=\"modal-dialog modal-lg\">\r\n          <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"!statusmodal\"\r\n              >\r\n                Tambah Pengguna\r\n              </h5>\r\n              <h5\r\n                class=\"modal-title\"\r\n                id=\"exampleModalLongTitle\"\r\n                v-show=\"statusmodal\"\r\n              >\r\n                Ubah Pengguna\r\n              </h5>\r\n              <button\r\n                type=\"button\"\r\n                class=\"close\"\r\n                data-dismiss=\"modal\"\r\n                aria-label=\"Close\"\r\n              >\r\n                <span aria-hidden=\"true\" @click=\"resetForm()\">&times;</span>\r\n              </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n              <div class=\"card card-default\">\r\n                <!-- /.card-header -->\r\n                <!-- form start -->\r\n                <form\r\n                  @submit.prevent=\"statusmodal ? updateData() : simpanData()\"\r\n                >\r\n                  <div class=\"card-body\">\r\n                    <p v-if=\"message\" class=\"alert-danger\">\r\n                      {{ message }}\r\n                    </p>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"exampleInputUsername1\">Username</label>\r\n                      <input\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputUsername1\"\r\n                        placeholder=\"Username\"\r\n                        v-model=\"form.username\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"exampleInputEmail1\">Email address</label>\r\n                      <input\r\n                        type=\"email\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputEmail1\"\r\n                        placeholder=\"Enter email\"\r\n                        v-model=\"form.email\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"exampleInputPassword1\">Password</label>\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputPassword1\"\r\n                        placeholder=\"Password\"\r\n                        v-model=\"form.password\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                      <label for=\"exampleInputPassword1\"\r\n                        >Confirmasi Password</label\r\n                      >\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        id=\"exampleInputPassword1\"\r\n                        placeholder=\"Confirmasi Password\"\r\n                        v-model=\"form.confPassword\"\r\n                        required\r\n                      />\r\n                    </div>\r\n\r\n                    <div class=\"form-group\">\r\n                      <label>Role</label>\r\n                      <select\r\n                        class=\"form-control select2\"\r\n                        v-model=\"form.role\"\r\n                        required\r\n                      >\r\n                        <option disabled value=\"\">-- Pilih --</option>\r\n                        <!-- <option value=\"admin\">Admin</option> -->\r\n                        <option value=\"guru\">Guru</option>\r\n                        <!-- <option value=\"walas\">Walas</option> -->\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"form-group\" v-if=\"guru\">\r\n                      <label for=\"nip\">NIP</label>\r\n                      <input\r\n                        type=\"number\"\r\n                        class=\"form-control\"\r\n                        id=\"nip\"\r\n                        placeholder=\"NIP\"\r\n                        v-model=\"form.nip\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\" v-if=\"guru\">\r\n                      <label for=\"name\">Name</label>\r\n                      <input\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        id=\"name\"\r\n                        placeholder=\"Name\"\r\n                        v-model=\"form.name\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\" v-if=\"guru\">\r\n                      <label for=\"tgl_lahir\">Tanggal Lahir</label>\r\n                      <input\r\n                        type=\"date\"\r\n                        class=\"form-control\"\r\n                        id=\"tgl_lahir\"\r\n                        placeholder=\"Tanggal Lahir\"\r\n                        v-model=\"form.tgl_lahir\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\" v-if=\"guru\">\r\n                      <label>Jenis Kelamin</label>\r\n                      <select\r\n                        class=\"form-control select2\"\r\n                        v-model=\"form.jkg\"\r\n                        required\r\n                      >\r\n                        <option disabled value=\"\">-- Pilih --</option>\r\n                        <option value=\"laki-laki\">Laki-Laki</option>\r\n                        <option value=\"perempuan\">Perempuan</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"form-group\" v-if=\"guru\">\r\n                      <label for=\"alamat\">Alamat</label>\r\n                      <input\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        id=\"alamat\"\r\n                        placeholder=\"Alamat\"\r\n                        v-model=\"form.alamat\"\r\n                        required\r\n                      />\r\n                    </div>\r\n                    <div class=\"form-group\" v-if=\"guru\">\r\n                      <label>Mengajar</label>\r\n                      <select\r\n                        class=\"form-control select2\"\r\n                        v-model=\"form.mapelId\"\r\n                        required\r\n                      >\r\n                        <option disabled value=\"\">-- Pilih --</option>\r\n                        <option\r\n                          v-for=\"mapel in mapels\"\r\n                          :key=\"mapel.id\"\r\n                          :value=\"mapel.id\"\r\n                        >\r\n                          {{ mapel.name_mapel }}\r\n                        </option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"modal-footer\">\r\n                    <button\r\n                      type=\"button\"\r\n                      class=\"btn btn-secondary\"\r\n                      data-dismiss=\"modal\"\r\n                      @click=\"resetForm()\"\r\n                    >\r\n                      Close\r\n                    </button>\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn btn-primary\"\r\n                      :disabled=\"disabled\"\r\n                      v-show=\"!statusmodal\"\r\n                    >\r\n                      <i v-show=\"loading\" class=\"fa fa-spinner fa-spin\"></i>\r\n                      Simpan\r\n                    </button>\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn btn-success\"\r\n                      :disabled=\"disabled\"\r\n                      v-show=\"statusmodal\"\r\n                    >\r\n                      <i v-show=\"loading\" class=\"fa fa-spinner fa-spin\"></i>\r\n                      Ubah\r\n                    </button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport User from \"../../services/user.service\";\r\nimport MapelService from \"../../services/mapel.service\";\r\nimport KelasService from \"../../services/kelas.service\";\r\nimport Swal from \"sweetalert2\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      disabled: false,\r\n      users: {},\r\n      mapels: {},\r\n      statusmodal: false,\r\n      message: \"\",\r\n      form: {\r\n        id: \"\",\r\n        username: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n        confPassword: \"\",\r\n        role: \"\",\r\n        nip: \"\",\r\n        name: \"\",\r\n        tgl_lahir: \"\",\r\n        jkg: \"\",\r\n        alamat: \"\",\r\n        mapelId: null,\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      return this.$store.state.auth.user;\r\n    },\r\n    guru() {\r\n      if (this.form && this.form.role) {\r\n        return this.form.role === \"guru\";\r\n      }\r\n      return false;\r\n    },\r\n  },\r\n  created() {\r\n    this.getdata();\r\n  },\r\n  methods: {\r\n    showModal() {\r\n      this.statusmodal = false;\r\n      this.resetForm();\r\n      $(\"#modalmuncul\").modal(\"show\");\r\n    },\r\n    showModalEdit(user) {\r\n      this.statusmodal = true;\r\n      $(\"#modalmuncul\").modal(\"show\");\r\n      this.getUserById(user);\r\n    },\r\n    getdata() {\r\n      this.$Progress.start();\r\n      User.getUser()\r\n        .then((response) => {\r\n          this.users = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n      MapelService.getMapel()\r\n        .then((response) => {\r\n          this.mapels = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n      KelasService.getKelas()\r\n        .then((response) => {\r\n          this.kelas = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n      this.$Progress.finish();\r\n    },\r\n    getUserById(id) {\r\n      User.getUserById(id)\r\n        .then((response) => {\r\n          this.form = response.data;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    simpanData() {\r\n      this.$Progress.start();\r\n      this.loading = true;\r\n      this.disabled = true;\r\n      User.createUser(this.form)\r\n        .then((response) => {\r\n          this.from = response.data;\r\n          console.log(response.data);\r\n          this.getdata();\r\n          this.resetForm();\r\n          $(\"#modalmuncul\").modal(\"hide\");\r\n          const Toast = Swal.mixin({\r\n            toast: true,\r\n            position: \"top-end\",\r\n            showConfirmButton: false,\r\n            timer: 3000,\r\n            timerProgressBar: true,\r\n            didOpen: (toast) => {\r\n              toast.addEventListener(\"mouseenter\", Swal.stopTimer);\r\n              toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\r\n            },\r\n          });\r\n\r\n          Toast.fire({\r\n            icon: \"success\",\r\n            title: \"Data Berhasil Tersimpan\",\r\n          });\r\n          this.$Progress.finish();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n        })\r\n        .catch((e) => {\r\n          this.$Progress.fail();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n          console.log(e);\r\n          this.message =\r\n            (e.response && e.response.data && e.response.data.message) ||\r\n            e.message ||\r\n            e.toString();\r\n        });\r\n    },\r\n    updateData() {\r\n      this.$Progress.start();\r\n      this.loading = true;\r\n      this.disabled = true;\r\n      User.updateUser(this.form.id, this.form)\r\n        .then((response) => {\r\n          this.from = response.data;\r\n          console.log(response.data);\r\n          this.getdata();\r\n          this.resetForm();\r\n          $(\"#modalmuncul\").modal(\"hide\");\r\n          const Toast = Swal.mixin({\r\n            toast: true,\r\n            position: \"top-end\",\r\n            showConfirmButton: false,\r\n            timer: 3000,\r\n            timerProgressBar: true,\r\n            didOpen: (toast) => {\r\n              toast.addEventListener(\"mouseenter\", Swal.stopTimer);\r\n              toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\r\n            },\r\n          });\r\n\r\n          Toast.fire({\r\n            icon: \"success\",\r\n            title: \"Data Berhasil Terupdate\",\r\n          });\r\n          this.$Progress.finish();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n        })\r\n        .catch((e) => {\r\n          this.$Progress.fail();\r\n          this.loading = false;\r\n          this.disabled = false;\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    deleteData(id) {\r\n      Swal.fire({\r\n        title: \"Anda Yakin Ingin Menghapus Data Ini ?\",\r\n        text: \"Klik Batal Untuk Membatalkan Penghapusan\",\r\n        icon: \"warning\",\r\n        showCancelButton: true,\r\n        confirmButtonColor: \"#3085d6\",\r\n        cancelButtonColor: \"#d33\",\r\n        confirmButtonText: \"Hapus\",\r\n      }).then((result) => {\r\n        if (result.value) {\r\n          User.deleteUser(id)\r\n            .then((response) => {\r\n              Swal.fire(\"Terhapus\", \"Data Anda Sudah Terhapus\", \"success\");\r\n              console.log(response.data);\r\n              this.getdata();\r\n            })\r\n            .catch(() => {\r\n              Swal.fire(\"Gagal\", \"Data Gagal Terhapus\", \"warning\");\r\n            });\r\n        }\r\n      });\r\n    },\r\n\r\n    resetForm() {\r\n      (this.form.username = \"\"),\r\n        (this.form.email = \"\"),\r\n        (this.form.password = \"\"),\r\n        (this.form.confPassword = \"\"),\r\n        (this.form.role = \"\"),\r\n        (this.form.nip = \"\"),\r\n        (this.form.name = \"\"),\r\n        (this.form.tgl_lahir = \"\"),\r\n        (this.form.jkg = \"\"),\r\n        (this.form.alamat = \"\"),\r\n        (this.form.mapelId = null);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}