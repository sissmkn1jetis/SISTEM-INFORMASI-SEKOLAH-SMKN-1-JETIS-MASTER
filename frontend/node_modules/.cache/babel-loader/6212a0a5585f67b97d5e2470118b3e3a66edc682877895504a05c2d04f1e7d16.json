{"ast":null,"code":"import User from \"../../services/user.service\";\nimport Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      users: {},\n      form: {\n        id: \"\",\n        username: \"\",\n        email: \"\",\n        password: \"\",\n        confPassword: \"\",\n        role: \"\"\n      },\n      edit: false\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  created() {\n    this.getdata();\n  },\n  methods: {\n    showModal() {\n      $(\"#modalmuncul\").modal(\"show\");\n    },\n    getdata() {\n      User.getUser().then(response => {\n        this.users = response.data;\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    simpanData() {\n      var data = {\n        username: this.form.username,\n        email: this.form.email,\n        password: this.form.password,\n        confPassword: this.form.confPassword,\n        role: this.form.role\n      };\n      User.create(data).then(() => {\n        this.getdata();\n        this.resetForm();\n        $(\"#modalmuncul\").modal(\"hide\");\n        const Toast = Swal.mixin({\n          toast: true,\n          position: \"top-end\",\n          showConfirmButton: false,\n          timer: 3000,\n          timerProgressBar: true,\n          didOpen: toast => {\n            toast.addEventListener(\"mouseenter\", Swal.stopTimer);\n            toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n          }\n        });\n        Toast.fire({\n          icon: \"success\",\n          title: \"Data Berhasil Tersimpan\"\n        });\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    updateData(form) {\n      this.form.id = form.id;\n      this.form.username = form.username;\n      this.form.email = form.email;\n      this.form.password = form.password;\n      this.form.confPassword = form.confPassword;\n      this.form.role = form.role;\n      this.edit = true;\n      this.showModal();\n      User.update(this.form.id, data).then(() => {\n        this.getdata();\n        this.resetForm();\n        $(\"#modalmuncul\").modal(\"hide\");\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n    deleteData(form) {\n      let keputusan = confirm(\"Apakah anda yakin ingin menghapus data\");\n      if (keputusan === true) {\n        User.delete(form.id).then(() => {\n          this.getdata();\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    },\n    resetForm() {\n      this.form.username = \"\", this.form.email = \"\", this.form.password = \"\", this.form.confPassword = \"\", this.form.role = \"\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAmLA,OAAOA,IAAG,MAAO,6BAA6B;AAC9C,OAAOC,IAAG,MAAO,aAAa;AAC9B,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC;MACTC,IAAI,EAAE;QACJC,EAAE,EAAE,EAAE;QACNC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,IAAI,EAAE;MACR,CAAC;MACDC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,IAAI,CAACC,IAAI;IACpC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACC,OAAO,EAAE;EAChB,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACVC,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;IACjC,CAAC;IACDJ,OAAO,GAAG;MACRnB,IAAI,CAACwB,OAAO,EAAC,CACVC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACvB,KAAI,GAAIuB,QAAQ,CAACxB,IAAI;MAC5B,CAAC,EACAyB,KAAK,CAAEC,CAAC,IAAK;QACZC,OAAO,CAACC,GAAG,CAACF,CAAC,CAAC;MAChB,CAAC,CAAC;IACN,CAAC;IACDG,UAAU,GAAG;MACX,IAAI7B,IAAG,GAAI;QACTI,QAAQ,EAAE,IAAI,CAACF,IAAI,CAACE,QAAQ;QAC5BC,KAAK,EAAE,IAAI,CAACH,IAAI,CAACG,KAAK;QACtBC,QAAQ,EAAE,IAAI,CAACJ,IAAI,CAACI,QAAQ;QAC5BC,YAAY,EAAE,IAAI,CAACL,IAAI,CAACK,YAAY;QACpCC,IAAI,EAAE,IAAI,CAACN,IAAI,CAACM;MAClB,CAAC;MAEDV,IAAI,CAACgC,MAAM,CAAC9B,IAAI,EACbuB,IAAI,CAAC,MAAM;QACV,IAAI,CAACN,OAAO,EAAE;QACd,IAAI,CAACc,SAAS,EAAE;QAChBX,CAAC,CAAC,cAAc,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;QAC/B,MAAMW,KAAI,GAAIjC,IAAI,CAACkC,KAAK,CAAC;UACvBC,KAAK,EAAE,IAAI;UACXC,QAAQ,E